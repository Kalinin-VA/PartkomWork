	ДанныеЗаявок.Порядок,
	ДанныеЗаявок.СтрокаЗаявки,
	ДанныеЗаявок.Состояние,
	ВЫБОР
		КОГДА ДанныеЗаявок.Порядок = 1
			ТОГДА ДанныеЗаявок.Количество
		ИНАЧЕ 0
	КОНЕЦ КАК КоличествоВЗаявке,
	ВЫБОР
		КОГДА ДанныеЗаявок.Порядок = 5
			ТОГДА ДанныеЗаявок.Количество
		ИНАЧЕ 0
	КОНЕЦ КАК КоличествоВРезерве,
	ВЫБОР
		КОГДА ДанныеЗаявок.Порядок = 2
			ТОГДА ДанныеЗаявок.Количество
		ИНАЧЕ 0
	КОНЕЦ КАК КоличествоЗаказано,
	ВЫБОР
		КОГДА ДанныеЗаявок.Порядок = 5
			ТОГДА ДанныеЗаявок.Количество
		ИНАЧЕ 0
	КОНЕЦ КАК КоличествоПоступило,
	ВЫБОР
		КОГДА ДанныеЗаявок.Порядок = 7
			ТОГДА ДанныеЗаявок.Количество
		ИНАЧЕ 0
	КОНЕЦ КАК КоличествоОтгружено,
	ВЫБОР
		КОГДА ДанныеЗаявок.Отказ
			ТОГДА ДанныеЗаявок.Количество
		ИНАЧЕ 0
	КОНЕЦ КАК КоличествоОтказ,
	ДанныеЗаявок.ДатаИзменения,
	ДанныеЗаявок.Отказ,
	ДанныеЗаявок.Склад,
	ДанныеЗаявок.Номенклатура
ПОМЕСТИТЬ ВтДанные
ИЗ
	ДанныеЗаявок КАК ДанныеЗаявок
;

////////////////////////////////////////////////////////////////////////////////
