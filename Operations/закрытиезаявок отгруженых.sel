	ВтДанные.СтрокаЗаявки,
	СУММА(ВтДанные.КоличествоВЗаявке) КАК КоличествоВЗаявке,
	СУММА(ВтДанные.КоличествоВРезерве) КАК КоличествоВРезерве,
	СУММА(ВтДанные.КоличествоЗаказано) КАК КоличествоЗаказано,
	СУММА(ВтДанные.КоличествоПоступило) КАК КоличествоПоступило,
	СУММА(ВтДанные.КоличествоОтгружено) КАК КоличествоОтгружено,
	СУММА(ВтДанные.КоличествоОтказ) КАК КоличествоОтказ,
	ВтДанные.Склад
ПОМЕСТИТЬ ВтОбщий
ИЗ
	ВтДанные КАК ВтДанные

СГРУППИРОВАТЬ ПО
	ВтДанные.СтрокаЗаявки,
	ВтДанные.Склад
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ВтДанные.СтрокаЗаявки КАК СтрокаЗаявки,
	ВтДанные.Состояние,
	ВтДанные.Порядок КАК Порядок,
	ВтОбщий.КоличествоВЗаявке,
	ВтОбщий.КоличествоВРезерве,
	ВтОбщий.КоличествоЗаказано,
	ВтОбщий.КоличествоОтгружено,
	ВтОбщий.КоличествоОтказ,
	ВтОбщий.КоличествоПоступило,
	ВтДанные.ДатаИзменения,
	ВтДанные.СтрокаЗаявки.IDSite КАК IDSite,
	ВтДанные.Отказ КАК Отказ,
	ВтДанные.Склад,
	ВтДанные.Номенклатура
ИЗ
	ВтДанные КАК ВтДанные
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВтОбщий КАК ВтОбщий
