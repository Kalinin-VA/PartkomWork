	Если Значение.Дата &lt; Дата('20140721000000') Тогда
		ОграничениеПоДате = Истина;
	КонецЕсли;

	ОграничениеПоОрганизации = Ложь;

	ИНН = Значение.Организация.ИНН;
	КПП = Значение.Организация.КПП;
	//Если (ИНН = "5256063986" и КПП = "525350001") или (ИНН = "3443067465" и КПП = "344301001") Тогда
	//	ОграничениеПоОрганизации = Истина;
	//КонецЕсли;

	Если (ИНН = "5256063986" и КПП = "525350001") Тогда
		Если Значение.Дата &lt; Дата('20141013000000') тогда
			ОграничениеПоОрганизации = Истина;	
		КонецЕсли;
	ИначеЕсли (ИНН = "3443067465" и КПП = "344301001") Тогда
		Если Значение.Дата &lt; Дата('20141024000000') тогда
			ОграничениеПоОрганизации = Истина;	
		КонецЕсли
	КонецЕсли;
	
	Ограничение = ОграничениеПоДате ИЛИ ОграничениеПоОрганизации; 
		


	Если Не Ограничение Тогда
		Состав = Значение.Состав.Выгрузить();
		
		Если НЕ Значение.Состав.Итог("СуммаПриход") = 0 и НЕ Значение.Состав.Итог("СуммаРасход") = 0 Тогда
			
		Иначе
			
			Если Состав.Количество() &gt; 0 Тогда
				Стр = Значение.Состав[0];
				Если ЗначениеЗаполнено(Стр.СуммаРасход) Тогда
					
					ЭтоЗП = Ложь;
					Попытка
						ЭтоЗП = Стр.СтатьяДвиженияДенежныхСредств.СтатьяДДСБУ = Справочники.СтатьиДДСБУ.ОплатаТрудаИПрочиеВыплатыПерсоналу;
