				//НС.СчетРасходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(ЗаказНаряд.Ссылка,ВыборкаЗН.Номенклатура,,ВыборкаЗН.СтавкаНДС).СчетРасходов;
				НС.СчетРасходов = УправленческийУчет.ПолучитьСчетаУчетаНоменклатуры(ЗаказНаряд.Ссылка,ВыборкаЗН.Номенклатура,,ВыборкаЗН.СтавкаНДС,,,"Регламентированный").СчетРасходов;
			Исключение
				//НС.СчетРасходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(ЗаказНаряд.Ссылка,ВыборкаЗН.Номенклатура).СчетРасходов;
				НС.СчетРасходов = УправленческийУчет.ПолучитьСчетаУчетаНоменклатуры(ЗаказНаряд.Ссылка,ВыборкаЗН.Номенклатура,,,,,"Регламентированный").СчетРасходов;
			КонецПопытки;
			НС.Содержание = ВыборкаЗН.Номенклатура.НаименованиеПолное;
		КонецЦикла;
		
		
		Если Не СоздаватьРеализациюУслуг Тогда
			Если ТаблицаТовары.Количество() &lt;&gt; 0 Тогда
				ВходящийДанные=Новый Структура("Материалы",ТаблицаТовары);
				//ВходящийДанные=Новый Структура("Материалы",ТаблицаТоварыДавальческие);
				ВыгрузитьПоПравилу(ЗаказНаряд.Ссылка,,ВходящийДанные,,"ТребованиеНакладная");
			КонецЕсли;
		Иначе
			Если СоздаватьРеализациюТоваров Тогда
				Если ТаблицаТовары.Количество() &gt; 0 Тогда
					ВходящийДанные=Новый Структура;
					ВходящийДанные.Вставить("Товары",ТаблицаТовары);
					ВходящийДанные.Вставить("Услуги",ПустаяТаблицаУслуги);
					ВходящийДанные.Вставить("Склад","НезавершенноеПроизводство");
					ВыгрузитьПоПравилу(ЗаказНаряд.Ссылка,,ВходящийДанные,,"ЗаказНарядАСС");
				КонецЕсли;
				ВходящийДанныеУслуги=Новый Структура;
				ВходящийДанныеУслуги.Вставить("Товары",ПустаяТаблицаТовары);
				ВходящийДанныеУслуги.Вставить("Услуги",ТаблицаУслуги);
				ВходящийДанныеУслуги.Вставить("Склад",Справочники.СкладыКомпании.ПустаяСсылка());
				ВыгрузитьПоПравилу(ЗаказНаряд.Ссылка,,ВходящийДанныеУслуги,,"ЗаказНарядАСС");
			Иначе
				Если ТаблицаТовары.Количество() &lt;&gt; 0 или ТаблицаТоварыДавальческие.Количество() &lt;&gt; 0 Тогда
					ВходящийДанные=Новый Структура("Материалы",ТаблицаТовары);
					ВходящийДанные=Новый Структура("Материалы",ТаблицаТоварыДавальческие);
					ВыгрузитьПоПравилу(ЗаказНаряд.Ссылка,,ВходящийДанные,,"ТребованиеНакладная");
				КонецЕсли;
				ВыгрузитьПоПравилу(ЗаказНаряд.Ссылка,,,,"ЗаказНаряд");	
			КонецЕсли;
		КонецЕсли;
		Если ТаблицаТоварыМойки.Количество() &lt;&gt; 0 Тогда
			ВходящийДанные=Новый Структура("Материалы",ТаблицаТоварыМойки);
			ВыгрузитьПоПравилу(ЗаказНаряд.Ссылка,,ВходящийДанные,,"ТребованиеНакладная");
		КонецЕсли;
		
		
	КонецЕсли;
КонецЕсли;

Отказ = Истина;</Текст>
