	//	Объект.СубконтоКт2 = ДоговорКонтрагента;
	//	// m.krivoshein 2015-10-23 &gt;
	
	КонецЕсли;
	// m.krivoshein 2015-10-23 &gt;
	
	СубконтоКонтрагент = ВидыСубконто.Найти(ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.Контрагенты,"ВидСубконто");
