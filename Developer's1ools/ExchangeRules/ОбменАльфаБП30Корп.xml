			Параметры.СписокДокументовКПроведению.Добавить(Объект,ВремяДок);
		ИначеЕсли Объект.Дата &gt;= Параметры.ДатаОграниченияГлубиныВыгрузки Тогда
			Параметры.СписокДокументовНаУдаление.Добавить(Объект,ВремяДок);
