		Если СчетРасчетов.ВидыСубконто.Количество() &gt; 0 Тогда
			Для Н = 1 По СчетРасчетов.ВидыСубконто.Количество() Цикл
				Если СчетРасчетов.ВидыСубконто[Н-1].ВидСубконто = ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.Контрагенты Тогда
					Объект["СубконтоДт"+Н] = Объект.Контрагент;
				ИначеЕсли СчетРасчетов.ВидыСубконто[Н-1].ВидСубконто = ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.Договоры Тогда
					
					// m.krivoshein 2016-01-21 &lt;
					// Заявка 165830
					// проверим договор в шапке, если не заполнен возьмем из первой строки расшифровки платежа
					ДоговорКонтрагента = Объект.ДоговорКонтрагента;
					Если НЕ ЗначениеЗаполнено(ДоговорКонтрагента) И Объект.РасшифровкаПлатежа.Количество() &gt; 0 Тогда
						ДоговорКонтрагента = Объект.РасшифровкаПлатежа[0].ДоговорКонтрагента;		
					КонецЕсли;
					Объект["СубконтоДт"+Н] = ДоговорКонтрагента;
					// m.krivoshein 2016-01-21 &gt;
					
				ИначеЕсли СчетРасчетов.ВидыСубконто[Н-1].ВидСубконто = ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.ПрочиеДоходыИРасходы Тогда
					Объект["СубконтоДт"+Н] = ПараметрыОбъекта.Получить("СтатьяПрочихДоховРасходов");
				ИначеЕсли СчетРасчетов.ВидыСубконто[Н-1].ВидСубконто = ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.НоменклатурныеГруппы Тогда
					Объект["СубконтоДт"+Н] = ПараметрыОбъекта.Получить("НоменклатурнаяГруппа");
				ИначеЕсли СчетРасчетов.ВидыСубконто[Н-1].ВидСубконто = ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.СтатьиЗатрат Тогда
					Объект["СубконтоДт"+Н] = ПараметрыОбъекта.Получить("СтатьяЗатрат");
				// m.krivoshein	2016-02-16 &lt;
				ИначеЕсли СчетРасчетов.ВидыСубконто[Н-1].ВидСубконто = ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.БанковскиеСчета Тогда
					Объект["СубконтоДт"+Н] = Объект.СчетКонтрагента;
				ИначеЕсли СчетРасчетов.ВидыСубконто[Н-1].ВидСубконто = ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.СтатьиДвиженияДенежныхСредств Тогда
					Объект["СубконтоДт"+Н] = ПараметрыОбъекта.Получить("СтатьяДДС");
				// m.krivoshein	2016-02-16 &gt;
				КонецЕсли;
			КонецЦикла;
		КонецЕсли;
	КонецЕсли;
	

	
ИначеЕсли Объект.ВидОперации = Перечисления.ВидыОперацийСписаниеДенежныхСредств.ПеречислениеНалога Тогда
	
	Если Не ПараметрыОбъекта = Неопределено Тогда
		КодБК = ПараметрыОбъекта.Получить("КодБК");
		ПоказательОснования = ПараметрыОбъекта.Получить("ПоказательОснования");
		ПоказательТипа = ПараметрыОбъекта.Получить("ПоказательТипа");
		
		// m.krivoshein_2016-04-25 Заявка 208632 &lt;		
		Если ЗначениеЗаполнено(Объект.Налог) Тогда
			Объект.СчетУчетаРасчетовСКонтрагентом = Объект.Налог.СчетУчета;
		Иначе
			Объект.СчетУчетаРасчетовСКонтрагентом = Справочники.ВидыНалоговИПлатежейВБюджет.СчетУчетаРасчетовПоКБК(
					КодБК);
		КонецЕсли;
		// m.krivoshein_2016-04-25 &gt;
				
		// m.krivoshein_2016-02-02 Заявка 179890 &lt;
		СтрокаПоискаПоКБК = "";
		Если ЗначениеЗаполнено(Объект.Налог) Тогда
			СтрокаПоискаПоКБК = Объект.Налог.КодБК;	
		КонецЕсли;			
			
		Объект.ВидНалоговогоОбязательства = Справочники.ВидыНалоговИПлатежейВБюджет.ВидНалоговогоОбязательстваПоКБК(
												СтрокаПоискаПоКБК, Объект.Дата, Объект.Налог);
		// m.krivoshein_2016-02-02 &gt;
		
		АналитикаРасчетов = Документы.СписаниеСРасчетногоСчета.ДанныеЗаполненияАналитикиНаСчетеРасчетовСБюджетом(
			Объект.СчетУчетаРасчетовСКонтрагентом,
			Объект.Организация,
			Объект.Контрагент,
			"",
			Объект.Контрагент.КПП,
			КодБК,
			ПоказательОснования,
			ПоказательТипа,
			Перечисления.ВидыПлатежейВГосБюджет.Налог);
					
			ЗаполнитьЗначенияСвойств(Объект, АналитикаРасчетов);
			
			// m.krivosein_2016-09-12 Заявка № 285321 &lt;
			СчетРасчетов = Объект.СчетУчетаРасчетовСКонтрагентом;
			
			Если ЗначениеЗаполнено(СчетРасчетов) Тогда
				Если СчетРасчетов.ВидыСубконто.Количество() &gt; 0 Тогда
					Для Н = 1 По СчетРасчетов.ВидыСубконто.Количество() Цикл
						Если СчетРасчетов.ВидыСубконто[Н-1].ВидСубконто = ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.РегистрацияВНалоговомОргане Тогда
							Объект["СубконтоДт"+Н] = ПараметрыОбъекта.Получить("РегистрацияВНалоговомОргане");
						КонецЕсли;
					КонецЦикла;
				КонецЕсли;
			КонецЕсли;
			// m.krivosein_2016-09-12 
			
	КонецЕсли;

// m.krivoshein 2015-12-09 Заявка № 109872 &lt;
// карта Мамочкина И.А.
ИначеЕсли Объект.ВидОперации = Перечисления.ВидыОперацийСписаниеДенежныхСредств.ПереводНаДругойСчет
			И Объект.СчетКонтрагента.НомерСчета = "40702810500454331158" Тогда
	Объект.СчетУчетаРасчетовСКонтрагентом = ПланыСчетов.Хозрасчетный.ПрочиеСпециальныеСчета;
// m.krivoshein 2015-12-09 &gt;
КонецЕсли;

// m.krivoshein 2016-04-11 Заявка 214894 &lt;
// заполним налоговый период
Если ЗначениеЗаполнено(Объект.Налог) И ПлатежиВБюджетКлиентСерверПереопределяемый.ЭтоФиксированныеВзносы(Объект.Налог.ВидНалога) Тогда
	Объект.НалоговыйПериод = НачалоГода(Объект.Дата);
Иначе
	Объект.НалоговыйПериод = НачалоМесяца(ДобавитьМесяц(Объект.Дата, -1));
КонецЕсли;
// m.krivoshein 2016-04-11 &gt;</Текст>
