	Запрос = Новый Запрос;
	Запрос.УстановитьПараметр("МоментВремени", Источник.Дата);
	Запрос.УстановитьПараметр("Организация", Источник.Организация);
	
	Запрос.Текст = "ВЫБРАТЬ
	|	УчетнаяПолитикаОрганизацийСрезПоследних.ВключитьОбменПоПодразделениям,
	|	УчетнаяПолитикаОрганизацийСрезПоследних.УчетПодразделений,
	|	УчетнаяПолитикаОрганизацийСрезПоследних.СпособВыгрузкиПодразделений,
	|	УчетнаяПолитикаОрганизацийСрезПоследних.ПилотныйПроектФРБ
	|ИЗ
	|	РегистрСведений.УчетнаяПолитикаОрганизаций.СрезПоследних(&amp;МоментВремени, Организация = &amp;Организация) КАК УчетнаяПолитикаОрганизацийСрезПоследних";
	
	РезультатЗапроса = Запрос.Выполнить();
	Если НЕ РезультатЗапроса.Пустой() Тогда
		
		Выборка = РезультатЗапроса.Выбрать();
		Выборка.Следующий();
		
		Если Выборка.ВключитьОбменПоПодразделениям Тогда
			
			СтруктураПараметров = Новый Структура();
			СтруктураПараметров.Вставить("УчетПодразделений", Выборка.УчетПодразделений);
			СтруктураПараметров.Вставить("СпособВыгрузкиПодразделений", Выборка.СпособВыгрузкиПодразделений);
						
			Параметры.ПараметрыУчетнойПолитики = СтруктураПараметров;
						
		КонецЕсли;
		
		// Пилотный проект ФРБ
		Параметры.ПилотныйПроектФРБ = Выборка.ПилотныйПроектФРБ;
		
	КонецЕсли;
	
КонецЕсли;
	
	</Текст>
