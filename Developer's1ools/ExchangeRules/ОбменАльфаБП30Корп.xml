					// Кривошеин 2015-10-13 &lt;
					ИсходящиеДанные.Вставить("СтатьяДДСУУ",Стр.СтатьяДвиженияДенежныхСредств);
					// Кривошеин 2015-10-13 &gt;
					ИсходящиеДанные.Вставить("РасшифровкаПлатежа",Состав);
					ИсходящиеДанные.Вставить("ВидОперации",Стр.СтатьяДвиженияДенежныхСредств.СтатьяДДСБУ);
					ИсходящиеДанные.Вставить("СтатьяДвиженияДенежныхСредств",Стр.СтатьяДвиженияДенежныхСредств.СтатьяДДСБУ);
					ИсходящиеДанные.Вставить("СуммаДокумента", Состав.Итог("СуммаРасход"));
					ИсходящиеДанные.Вставить("Контрагент",?(ЭтоЗП,"",Стр.Контрагент));
					ИсходящиеДанные.Вставить("СчетКонтрагента",?(ЗначениеЗаполнено(Стр.БанковскийСчетКонтрагента),Стр.БанковскийСчетКонтрагента,Значение.БанковскийСчетКонтрагента) );
					Если Стр.СтатьяДвиженияДенежныхСредств.СтатьяДДСБУ = Справочники.СтатьиДДСБУ.ВыплатыПодотчетныхСумм Тогда
