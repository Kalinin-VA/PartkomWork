		<Алгоритм Имя="ОбновитьРеквизитыТребованиеНакладной" ИспользуетсяПриЗагрузке="true">
			<Текст>// m.krivosein_2016-09-12 Заявка № 286793 &lt;
Если Объект.ПометкаУдаления ИЛИ (НЕ Объект.Проведен) Тогда
	// найдем требование накладную
	Запрос = Новый Запрос;
	Запрос.УстановитьПараметр("ИД", Объект.ИД);
	Запрос.Текст = "ВЫБРАТЬ
	|	ТребованиеНакладная.Ссылка,
	|	ТребованиеНакладная.Дата,
	|	ТребованиеНакладная.ПометкаУдаления
	|ИЗ
	|	Документ.ТребованиеНакладная КАК ТребованиеНакладная
	|ГДЕ
	|	ТребованиеНакладная.ИД = &amp;ИД";
	
	РезультатЗапрос = Запрос.Выполнить();
	
	Если НЕ РезультатЗапрос.Пустой() Тогда
		
		Выборка = РезультатЗапрос.Выбрать();
		Выборка.Следующий();
		
		Если Объект.Дата &lt;&gt; Выборка.Дата Тогда
			
			Документ 						= Выборка.Ссылка.ПолучитьОбъект();
			Документ.Дата 					= Объект.Дата;
			Документ.Комментарий            = Объект.Комментарий;
			Документ.ОбменДанными.Загрузка 	= Истина;
			Документ.Записать();
			
		КонецЕсли;
		
		// добавим документ на удаление
		ВремяДок = Формат(Выборка.Дата, "ДФ=""гггг ММ дд   ЧЧ:мм:сс""");
		Параметры.СписокДокументовНаУдаление.Добавить(Выборка.Ссылка, ВремяДок);
		
	КонецЕсли;
	
КонецЕсли;
