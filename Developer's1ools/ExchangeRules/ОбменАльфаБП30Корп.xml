		ПоследовательностьМетаданные.Имя, Об.Метаданные().Имя);
		
		Если Не Запрос = Неопределено Тогда
			Если ЗначениеЗаполнено(Запрос.Текст) Тогда
				Запрос.УстановитьПараметр("Ссылка", Об.Ссылка);
				ВсеЗначенияИзмерений = Запрос.Выполнить().Выгрузить();
				
				ВсеЗначенияИзмерений.Колонки.Добавить("Период", Новый ОписаниеТипов("Дата", , , Новый КвалификаторыДаты(ЧастиДаты.ДатаВремя)));
				ВсеЗначенияИзмерений.ЗаполнитьЗначения(Об.Дата, "Период");
				Набор.Загрузить(ВсеЗначенияИзмерений);
				// Подготовленный набор запишется автоматически, поскольку мы находимся в обработчике, а он взят из документа
				
			КонецЕсли;
		КонецЕсли;
	КонецЕсли;
	Набор.Записать();
	
КонецЦикла;</Текст>
