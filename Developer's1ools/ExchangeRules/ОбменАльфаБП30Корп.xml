		<Алгоритм Имя="ЗаполнитьПлановуюЦенуТаблинойЧасти" ИспользуетсяПриЗагрузке="true">
			<Текст>
ПлановаяЦена = 0;

Запросы.ПолучитьПлановуюЦену.УстановитьПараметр("ДатаОстатков", Новый Граница(Объект.Дата, ВидГраницы.Включая))	;
Запросы.ПолучитьПлановуюЦену.УстановитьПараметр("СчетЗатрат", Объект.СчетЗатрат);
Запросы.ПолучитьПлановуюЦену.УстановитьПараметр("ПодразделениеЗатрат", Объект.ПодразделениеЗатрат);
Запросы.ПолучитьПлановуюЦену.УстановитьПараметр("НоменклатурнаяГруппа", Объект.НоменклатурнаяГруппа);

РезультатЗапроса = Запросы.ПолучитьПлановуюЦену.Выполнить();

Если Не РезультатЗапроса.Пустой() Тогда
	Выборка = РезультатЗапроса.Выбрать();
	Выборка.Следующий();
	ПлановаяЦена = Выборка.СуммаОстатокДт;
КонецЕсли;

ТабЧасть = Объект.Услуги;

ОсталосьРаспределенить = ПлановаяЦена;
СуммаДокумента = ТабЧасть.Итог("Сумма");
КоэффициентРаспределения = 0;
КоличествоСтрок = ТабЧасть.Количество();

Если ПлановаяЦена &gt; 0 И СуммаДокумента &gt; 0 Тогда

	Для каждого СтрокаТабличнойЧасти ИЗ ТабЧасть Цикл
		
		Если КоличествоСтрок = СтрокаТабличнойЧасти.НомерСтроки Тогда
			СуммаПлановая = ОсталосьРаспределенить;
		Иначе
			КоэффициентРаспределения = СтрокаТабличнойЧасти.Сумма / СуммаДокумента;
			СуммаПлановая = Окр(КоэффициентРаспределения * ПлановаяЦена, 2);
		КонецЕсли;
			
		СтрокаТабличнойЧасти.СуммаПлановая = СуммаПлановая;
		СтрокаТабличнойЧасти.ПлановаяСтоимость = Окр(СуммаПлановая / СтрокаТабличнойЧасти.Количество, 2);
		ОсталосьРаспределенить = ОсталосьРаспределенить - СуммаПлановая;
		
	КонецЦикла;

КонецЕсли;</Текст>
			<Параметры>Объект, Отказ</Параметры>
		</Алгоритм>
