	Запрос.УстановитьПараметр("ВидДвиженияПоПартиям",ВидДвиженияНакопления.Приход);
Иначе
	ЗнакДвиженияПоПартиям="";
	Запрос.УстановитьПараметр("ВидДвиженияПоПартиям",ВидДвиженияНакопления.Расход);
