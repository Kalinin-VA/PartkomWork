	Группа2 = Справочники.СтатьиДвиженияДенежныхСредств.НайтиПоРеквизиту("Код2014","03.500.00");
	Группа3 = Справочники.СтатьиДвиженияДенежныхСредств.НайтиПоРеквизиту("Код2014","04.500.00");
	Если Источник.ХозОперация = Справочники.ХозОперации.ВозвратДенежныхСредствОтПодотчетника Тогда
		Значение = Справочники.СтатьиДДСБУ.ВозвратПодотчетныхСумм;
	ИначеЕсли Значение.ПринадлежитЭлементу(Группа1) или Значение.ПринадлежитЭлементу(Группа2) или Значение.ПринадлежитЭлементу(Группа3) Тогда
		Значение = Справочники.СтатьиДДСБУ.ВозвратыОтПоставщиков;
	Иначе
		Значение = Значение.СтатьяДДСБУ;
	КонецЕсли;
Иначе
	Значение = Справочники.СтатьиДДСБУ.ПустаяСсылка();
КонецЕсли;</ПриВыгрузке>
