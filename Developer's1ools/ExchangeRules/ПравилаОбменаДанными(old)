			<Порядок>150</Порядок>
			<Правило Отключить="false">
				<Код>СчетаУчетаРасчетовСКонтрагентами</Код>
				<Наименование>СчетаУчетаРасчетовСКонтрагентами</Наименование>
				<Порядок>50</Порядок>
				<КодПравилаКонвертации>СчетаУчетаРасчетовСКонтрагентамиУУ                </КодПравилаКонвертации>
				<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
				<ОбъектВыборки>РегистрСведенийЗапись.СчетаУчетаРасчетовСКонтрагентами</ОбъектВыборки>
				<ПередВыгрузкойОбъекта>Регистратор = Объект.Отбор.Регистратор.Значение;
Организация = Регистратор.Организация;
ПодразделениеКомпании = Регистратор.ПодразделениеКомпании;
Период = Регистратор.Дата;

ВД = Новый Структура;
ВД.Вставить("Ссылка", Регистратор);
ВД.Вставить("Период", Период);
ВД.Вставить("Организация", Организация);
ВД.Вставить("ПодразделениеКомпании",ПодразделениеКомпании);

