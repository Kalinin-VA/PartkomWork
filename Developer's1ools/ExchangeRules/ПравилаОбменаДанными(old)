				|	И Не Товары.Номенклатура.ЕдиныйКлассификаторНоменклатурыАвтозапчастей = &amp;ЗначениеРМНормируемых";
				
Запрос.УстановитьПараметр("Ссылка",Источник.Ссылка);
Попытка 
	Запрос.УстановитьПараметр("ЗначениеРМНормируемых",Справочники.ЕдиныйКлассификаторНоменклатурыАвтозапчастей.НормируемыеРМ);	
Исключение
	Запрос.УстановитьПараметр("ЗначениеРМНормируемых",Справочники.ЕдиныйКлассификаторНоменклатурыАвтозапчастей.ПустаяСсылка());
КонецПопытки;

Товары = Запрос.Выполнить().Выгрузить();
				
