КонецЕсли;

ТаблицаТовары=Новый ТаблицаЗначений;
ТаблицаТовары.Колонки.Добавить("Номенклатура",Новый ОписаниеТипов("СправочникСсылка.Номенклатура"));
ТаблицаТовары.Колонки.Добавить("Количество",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаТовары.Колонки.Добавить("Цена",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаТовары.Колонки.Добавить("Сумма",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаТовары.Колонки.Добавить("СуммаНДС",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаТовары.Колонки.Добавить("СтавкаНДС");
ТаблицаТовары.Колонки.Добавить("СчетДоходов");
ТаблицаТовары.Колонки.Добавить("СчетРасходов");
ТаблицаТовары.Колонки.Добавить("СчетУчетаНДСПоРеализации");
ТаблицаТовары.Колонки.Добавить("Содержание");
ТаблицаТовары.Колонки.Добавить("Субконто");

НоваяСтрока=ТаблицаТовары.Добавить();
НоваяСтрока.Номенклатура=Справочники.Номенклатура.Авторабота;
НоваяСтрока.Количество=1;
НоваяСтрока.Цена=Объект.СуммаДокумента;
НоваяСтрока.Сумма=Объект.СуммаДокумента;
НоваяСтрока.СуммаНДС =0;
НоваяСтрока.СтавкаНДС = Справочники.СтавкиНДС.БезНДС;
НоваяСтрока.СчетДоходов ="";
НоваяСтрока.СчетРасходов = "";
НоваяСтрока.СчетУчетаНДСПоРеализации= "";
НоваяСтрока.Содержание = "";
НоваяСтрока.Субконто="";

//Если Объект.Организация.ИНН = Объект.Контрагент.ИНН и Объект.Организация.КПП = Объект.Контрагент.КПП Тогда
//	АктНеНужен = Истина;
//Иначе
//	АктНеНужен = Ложь;
//КонецЕсли;
ВидОтношений = Объект.Контрагент.ВидКонтрагента;
ВидОтношенийВнутренний = (ВидОтношений = Перечисления.ВидыКонтрагентов.ВнутреннееПодразделение);
Если ВидОтношенийВнутренний Тогда
	АктНеНужен = Истина;
Иначе
	АктНеНужен = Ложь;
КонецЕсли;

ВходящийДанные=Новый Структура;
ВходящийДанные.Вставить("Услуги",ТаблицаТовары);

Если Не АктНеНужен Тогда
	ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящийДанные,,"АктОбОказанииУслугМойкиРеализация");
КонецЕсли;

Отказ = Истина;</ПередВыгрузкойОбъекта>
				</Правило>
				<Правило Отключить="false">
					<Код>МойкаАвтомобиля</Код>
