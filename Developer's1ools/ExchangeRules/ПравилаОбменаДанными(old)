					</Правило>
				</Группа>
				<Группа Отключить="false">
					<Код>Инвентаризация</Код>
					<Наименование>Инвентаризация</Наименование>
					<Порядок>350</Порядок>
					<Правило Отключить="false">
						<Код>ПересортицаТоваров</Код>
						<Наименование>ПересортицаТоваров</Наименование>
						<Порядок>50</Порядок>
						<КодПравилаКонвертации>ПересортицаТоваров                                </КодПравилаКонвертации>
						<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
						<ОбъектВыборки>ДокументСсылка.ПересортицаТоваров</ОбъектВыборки>
						<ПередВыгрузкойОбъекта>ТаблицаОприходования=Новый ТаблицаЗначений;
ТаблицаОприходования.Колонки.Добавить("Номенклатура",Новый ОписаниеТипов("СправочникСсылка.Номенклатура"));
ТаблицаОприходования.Колонки.Добавить("Количество",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаОприходования.Колонки.Добавить("Сумма",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаОприходования.Колонки.Добавить("Цена",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаОприходования.Колонки.Добавить("СчетУчета",Новый ОписаниеТипов("ПланСчетовСсылка.Регламентированный"));
ТаблицаОприходования.Колонки.Добавить("Коэффициент",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаОприходования.Колонки.Добавить("ЕдиницаИзмерения",Новый ОписаниеТипов("СправочникСсылка.ЕдиницыИзмерения"));

ТаблицаСписания=Новый ТаблицаЗначений;
ТаблицаСписания.Колонки.Добавить("Номенклатура",Новый ОписаниеТипов("СправочникСсылка.Номенклатура"));
ТаблицаСписания.Колонки.Добавить("Количество",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаСписания.Колонки.Добавить("Сумма",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаСписания.Колонки.Добавить("Цена",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаСписания.Колонки.Добавить("СчетУчета",Новый ОписаниеТипов("ПланСчетовСсылка.Регламентированный"));
ТаблицаСписания.Колонки.Добавить("Коэффициент",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаСписания.Колонки.Добавить("ЕдиницаИзмерения",Новый ОписаниеТипов("СправочникСсылка.ЕдиницыИзмерения"));

ТаблицаИнвентаризация=Новый ТаблицаЗначений;
ТаблицаИнвентаризация.Колонки.Добавить("Номенклатура",Новый ОписаниеТипов("СправочникСсылка.Номенклатура"));
ТаблицаИнвентаризация.Колонки.Добавить("Количество",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаИнвентаризация.Колонки.Добавить("КоличествоУчет",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаИнвентаризация.Колонки.Добавить("Сумма",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаИнвентаризация.Колонки.Добавить("СуммаУчет",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаИнвентаризация.Колонки.Добавить("Цена",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаИнвентаризация.Колонки.Добавить("СчетУчета",Новый ОписаниеТипов("ПланСчетовСсылка.Регламентированный"));

ДокОбъект = Объект;

Для Каждого СтрокаИнв Из Объект.Товары Цикл
	СуммаСписания = дкПолучитьСуммуСписания(ДокОбъект, "ПартииТоваровКомпании",
			СтрокаИнв.НоменклатураРасход,
			СтрокаИнв.ХарактеристикаНоменклатурыРасход);
	
	НС = ТаблицаОприходования.Добавить();
	НС.Номенклатура = СтрокаИнв.Номенклатура;
	НС.Коэффициент = 1;
	НС.Количество = СтрокаИнв.Количество;
	НС.Сумма = СтрокаИнв.Сумма;
	НС.Цена = ?(СтрокаИнв.Количество = 0, 0, СтрокаИнв.Сумма/СтрокаИнв.Количество);
	НС.СчетУчета = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,СтрокаИнв.Номенклатура).СчетУчета;
	НС.ЕдиницаИзмерения = СтрокаИнв.ЕдиницаИзмерения;
	
	НС = ТаблицаСписания.Добавить();
	НС.Номенклатура = СтрокаИнв.НоменклатураРасход;
	НС.Коэффициент = 1;
	НС.Количество = СтрокаИнв.КоличествоРасход;
	НС.Сумма = СуммаСписания;
	НС.Цена = ?(СтрокаИнв.КоличествоРасход = 0, 0, СуммаСписания/СтрокаИнв.КоличествоРасход);
	НС.СчетУчета = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,СтрокаИнв.НоменклатураРасход).СчетУчета;
	НС.ЕдиницаИзмерения = СтрокаИнв.ЕдиницаИзмеренияРасход;
	
	НС = ТаблицаИнвентаризация.Добавить();
	НС.Номенклатура = СтрокаИнв.НоменклатураРасход;
	НС.Количество = 0;
	НС.КоличествоУчет = СтрокаИнв.КоличествоРасход;
	НС.Сумма = 0;
	НС.СуммаУчет = СуммаСписания;
	НС.Цена = ?(СтрокаИнв.КоличествоРасход = 0, 0, СуммаСписания/СтрокаИнв.КоличествоРасход);
	НС.СчетУчета = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,СтрокаИнв.НоменклатураРасход).СчетУчета;
	
	НС = ТаблицаИнвентаризация.Добавить();
	НС.Номенклатура = СтрокаИнв.Номенклатура;
	НС.Количество = СтрокаИнв.Количество;
	НС.КоличествоУчет = 0;
	НС.Сумма = СтрокаИнв.Сумма;
	НС.СуммаУчет = 0;
	НС.Цена = ?(СтрокаИнв.Количество = 0, 0, СтрокаИнв.Сумма/СтрокаИнв.Количество);
	НС.СчетУчета = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,СтрокаИнв.Номенклатура).СчетУчета;
	
	
		
КонецЦикла;


ВходящиеДанные=Новый Структура;
ВходящиеДанные.Вставить("Товары",ТаблицаИнвентаризация);
ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящиеДанные,,"ПересортицаТоваров");
	

ВходящиеДанные=Новый Структура;
ВходящиеДанные.Вставить("Товары",ТаблицаОприходования);
ВходящиеДанные.Вставить("ИнвентаризацияТоваровНаСкладе",Объект.Ссылка);
ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящиеДанные,,"ИнвентаризацияОприходованиеТоваров");

ВходящиеДанные=Новый Структура;
ВходящиеДанные.Вставить("Товары",ТаблицаСписания);
ВходящиеДанные.Вставить("ИнвентаризацияТоваровНаСкладе",Объект.Ссылка);
ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящиеДанные,,"ИнвентаризацияСписаниеТоваров");

Отказ = Истина;</ПередВыгрузкойОбъекта>
					</Правило>
					<Правило Отключить="false">
						<Код>ИнвентаризацияТоваровНаСкладе</Код>
						<Наименование>ИнвентаризацияТоваровНаСкладе</Наименование>
						<Порядок>50</Порядок>
						<КодПравилаКонвертации>ИнвентаризацияТоваровНаСкладе                     </КодПравилаКонвертации>
						<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
						<ОбъектВыборки>ДокументСсылка.Инвентаризация</ОбъектВыборки>
						<ПередВыгрузкойОбъекта>ТаблицаОприходования=Новый ТаблицаЗначений;
ТаблицаОприходования.Колонки.Добавить("Номенклатура",Новый ОписаниеТипов("СправочникСсылка.Номенклатура"));
ТаблицаОприходования.Колонки.Добавить("Количество",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаОприходования.Колонки.Добавить("Сумма",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаОприходования.Колонки.Добавить("Цена",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаОприходования.Колонки.Добавить("СчетУчета",Новый ОписаниеТипов("ПланСчетовСсылка.Регламентированный"));
ТаблицаОприходования.Колонки.Добавить("Коэффициент",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаОприходования.Колонки.Добавить("ЕдиницаИзмерения",Новый ОписаниеТипов("СправочникСсылка.ЕдиницыИзмерения"));

ТаблицаСписания=Новый ТаблицаЗначений;
ТаблицаСписания.Колонки.Добавить("Номенклатура",Новый ОписаниеТипов("СправочникСсылка.Номенклатура"));
ТаблицаСписания.Колонки.Добавить("Количество",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаСписания.Колонки.Добавить("Сумма",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаСписания.Колонки.Добавить("Цена",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаСписания.Колонки.Добавить("СчетУчета",Новый ОписаниеТипов("ПланСчетовСсылка.Регламентированный"));
ТаблицаСписания.Колонки.Добавить("Коэффициент",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
ТаблицаСписания.Колонки.Добавить("ЕдиницаИзмерения",Новый ОписаниеТипов("СправочникСсылка.ЕдиницыИзмерения"));



Для Каждого СтрокаИнв Из Объект.Товары Цикл
	Если СтрокаИнв.КоличествоФакт &gt; СтрокаИнв.КоличествоКнижн Тогда
		НС = ТаблицаОприходования.Добавить();
		НС.Номенклатура = СтрокаИнв.Номенклатура;
		НС.Коэффициент = 1;
		НС.Количество = СтрокаИнв.КоличествоФакт - СтрокаИнв.КоличествоКнижн;
		НС.Сумма = СтрокаИнв.СуммаФакт - СтрокаИнв.СуммаКнижн;
		НС.Цена = СтрокаИнв.Цена;
		НС.СчетУчета = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,СтрокаИнв.Номенклатура).СчетУчета;
		НС.ЕдиницаИзмерения = СтрокаИнв.Номенклатура.БазоваяЕдиницаИзмерения;
	ИначеЕсли СтрокаИнв.КоличествоФакт &lt; СтрокаИнв.КоличествоКнижн Тогда
		НС = ТаблицаСписания.Добавить();
		НС.Номенклатура = СтрокаИнв.Номенклатура;
		НС.Коэффициент = 1;
		НС.Количество = СтрокаИнв.КоличествоКнижн - СтрокаИнв.КоличествоФакт;
		НС.Сумма = СтрокаИнв.СуммаКнижн - СтрокаИнв.СуммаФакт;
		НС.Цена = СтрокаИнв.Цена;
		НС.СчетУчета = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,СтрокаИнв.Номенклатура).СчетУчета;
		НС.ЕдиницаИзмерения = СтрокаИнв.Номенклатура.БазоваяЕдиницаИзмерения;
	КонецЕсли;
КонецЦикла;


ВыгрузитьПоПравилу(Объект.Ссылка,,,,"ИнвентаризацияТоваровНаСкладе");
	
Если ТаблицаОприходования.Количество() &lt;&gt; 0 Тогда
	ВходящиеДанные=Новый Структура;
	ВходящиеДанные.Вставить("Товары",ТаблицаОприходования);
	ВходящиеДанные.Вставить("ИнвентаризацияТоваровНаСкладе",Объект.Ссылка);
	
	ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящиеДанные,,"ИнвентаризацияОприходованиеТоваров");
КонецЕсли;

Если ТаблицаСписания.Количество() &lt;&gt; 0 Тогда
	ВходящиеДанные=Новый Структура;
	ВходящиеДанные.Вставить("Товары",ТаблицаСписания);
	ВходящиеДанные.Вставить("ИнвентаризацияТоваровНаСкладе",Объект.Ссылка);
	
	ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящиеДанные,,"ИнвентаризацияСписаниеТоваров");
КонецЕсли;

Отказ = Истина;</ПередВыгрузкойОбъекта>
					</Правило>
				</Группа>
				<Группа Отключить="false">
					<Код>Складские</Код>
					<Наименование>Складские</Наименование>
					<Порядок>400</Порядок>
					<Правило Отключить="false">
						<Код>Рузкомплектация</Код>
						<Наименование>Разукомплектация</Наименование>
						<Порядок>300</Порядок>
						<КодПравилаКонвертации>Разукомплектация                                  </КодПравилаКонвертации>
						<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
						<ОбъектВыборки>ДокументСсылка.Разукомплектация</ОбъектВыборки>
					</Правило>
					<Правило Отключить="false">
						<Код>Комплектация</Код>
