Иначе
	
	ЗапросАвто=новый Запрос;
	ЗапросАвто.Текст="
	|ВЫБРАТЬ
	|	ОстаткиАвтомобилейОстатки.КоличествоОстаток Как Колво
	|ИЗ
	|	РегистрНакопления.ОстаткиАвтомобилей.Остатки(&amp;ДатаНач, Автомобиль В (&amp;Автомобиль)) КАК ОстаткиАвтомобилейОстатки
	|ГДЕ
	|	ОстаткиАвтомобилейОстатки.КоличествоОстаток &gt; 0";
	НаМомент = Новый Граница(Источник.Дата,ВидГраницы.Включая);
	ЗапросАвто.УстановитьПараметр("ДатаНач",НаМомент);
	ЗапросАвто.УстановитьПараметр("Автомобиль",ОбъектКоллекции.Номенклатура);
	Если ЗапросАвто.Выполнить().Пустой() Тогда
		АвтомобильНаОстатках = Ложь;
	Иначе
		АвтомобильНаОстатках = Истина;
	КонецЕсли;
	
	Если АвтомобильНаОстатках Тогда
		Значение = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Источник,Справочники.Номенклатура.ПустаяСсылка()).СчетУчета;
	Иначе
		ЗапросРеал=новый Запрос;
		ЗапросРеал.Текст="ВЫБРАТЬ
		|	РеализацияАвтомобилейАвтомобили.Ссылка
		|ИЗ
