		<Алгоритм Имя="ОграничениеНаВыгрузкуПКО" ИспользуетсяПриЗагрузке="false">
			<Текст>Если Документы.ТипВсеСсылки().СодержитТип(ТипЗнч(Источник.Ссылка)) Тогда
	Если Источник.Ссылка.Дата&lt;Параметры.ДатаОграниченияГлубиныВыгрузки Тогда
		//Отказ = Истина;
		//ТолькоПолучитьУзелСсылки = Истина;
		НеЗамещатьОбъектПриЗагрузке = Истина;
