Если ТипЗнч(Источник.ДокументОснование) = Тип("ДокументСсылка.ПоступлениеТоваров") Тогда
	ИмяПКО = "ПоступлениеТоваровУслуг";
ИначеЕсли ТипЗнч(Источник.ДокументОснование) = Тип("ДокументСсылка.ВозвратОтПокупателя") Тогда
	ИмяПКО = "ВозвратТоваровОтПокупателя";
ИначеЕсли ТипЗнч(Источник.ДокументОснование) = Тип("ДокументСсылка.ПоступлениеДопРасходов") Тогда
	ХозОперацияДопРасходыТовары = Источник.ДокументОснование.ХозОперация = Справочники.ХозОперации.ПоступлениеДопРасходов;

	Если ХозОперацияДопРасходыТовары Тогда
		ИмяПКО = "ПоступлениеДопРасходовТовары";
	Иначе
        ЗапросАвто=новый Запрос;
		ЗапросАвто.Текст="
		|ВЫБРАТЬ
		|	ОстаткиАвтомобилейОстатки.КоличествоОстаток Как Колво
		|ИЗ
		|	РегистрНакопления.ОстаткиАвтомобилей.Остатки(&amp;ДатаНач, Автомобиль В (&amp;Автомобиль)) КАК ОстаткиАвтомобилейОстатки
		|ГДЕ
		|	ОстаткиАвтомобилейОстатки.КоличествоОстаток &gt; 0";
		НаМомент = Новый Граница(Источник.ДокументОснование.Дата,ВидГраницы.Включая);
		ЗапросАвто.УстановитьПараметр("ДатаНач",НаМомент);
		ЗапросАвто.УстановитьПараметр("Автомобиль",Источник.ДокументОснование.Автомобиль);
		Если ЗапросАвто.Выполнить().Пустой() Тогда
			АвтомобильНаОстатках = Ложь;
		Иначе
			АвтомобильНаОстатках = Истина;
		КонецЕсли;
		// Исключение пока убрано, по заявке 66762
		//ИсключениеГрузитьНеНадо = СОКРЛП(Объект.СтатьяДоходовИРасходовДляОбмена.НовыйКод) =  "05.01.14" //Статья Предпродажное сервисное обслуживание автомобилей
		//							ИЛИ 
									
		ИсключениеГрузитьНеНадо = Найти(Источник.ДокументОснование.Контрагент.ИНН,"7706640206")&gt;0; // Контрагент РАТ
		
		ИсключениеГрузитьНеНадо = ИсключениеГрузитьНеНадо ИЛИ СОКРЛП(Источник.ДокументОснование.СтатьяДоходовИРасходовДляОбмена.Код2014) =  "05.01.14";//Статья ДиР – Банковские услуги
									
		СтараяСтатьяИзменениеВин = СОКРЛП(Источник.ДокументОснование.СтатьяДоходовИРасходовДляОбмена.Код) =  "41.01.0116";
		ХозОперацияИзменениеВин = Источник.ДокументОснование.ХозОперация = Справочники.ХозОперации.ПереоборудованиеСИзменениемVIN;
		СтатьяНаСебестоимость = //СОКРЛП(Объект.СтатьяДоходовИРасходовДляОбмена.НовыйКод) = "05.01.12" //Транспортировка и доставка автомобилей
								//ИЛИ  Пока исключили
								СОКРЛП(Источник.ДокументОснование.СтатьяДоходовИРасходовДляОбмена.Код2014) = "05.01.13" //Расходы на таможню по автомобилям
								ИЛИ СОКРЛП(Источник.ДокументОснование.СтатьяДоходовИРасходовДляОбмена.Код2014) = "05.01.15" //Переоборудование автомобилей
								//Добавлены статьи по заявке №120701
								ИЛИ СОКРЛП(Источник.ДокументОснование.СтатьяДоходовИРасходовДляОбмена.Код2014) = "05.01.12" //Транспортировка и доставка автомобилей до склада
								ИЛИ СОКРЛП(Источник.ДокументОснование.СтатьяДоходовИРасходовДляОбмена.Код2014) = "05.01.16" //Дополнительное оборудование на автомобили
								ИЛИ СОКРЛП(Источник.ДокументОснование.СтатьяДоходовИРасходовДляОбмена.Код2014) = "07.25.10"; //Изменение себестоимости товарного автомобиля
		Если ИсключениеГрузитьНеНадо Тогда					
			Отказ = Истина;
		Иначе
			Если (СтараяСтатьяИзменениеВин или ХозОперацияИзменениеВин или СтатьяНаСебестоимость) и АвтомобильНаОстатках Тогда
				ТаблицаТовары=Новый ТаблицаЗначений;
				ТаблицаТовары.Колонки.Добавить("Номенклатура",Новый ОписаниеТипов("СправочникСсылка.Автомобили"));
				ТаблицаТовары.Колонки.Добавить("НоменклатураПосле",Новый ОписаниеТипов("СправочникСсылка.Автомобили"));
				ТаблицаТовары.Колонки.Добавить("Количество",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
				ТаблицаТовары.Колонки.Добавить("СчетУчета",Новый ОписаниеТипов("ПланСчетовСсылка.Регламентированный"));
				ТаблицаТовары.Колонки.Добавить("СчетУчетаНУ",Новый ОписаниеТипов("ПланСчетовСсылка.Регламентированный"));
				ТаблицаТовары.Колонки.Добавить("СчетУчетаНДС",Новый ОписаниеТипов("ПланСчетовСсылка.Регламентированный"));
				
				НС = ТаблицаТовары.Добавить();
				Если СтараяСтатьяИзменениеВин или ХозОперацияИзменениеВин Тогда
					НС.Номенклатура = Источник.ДокументОснование.АвтомобильДоДоработки;
					НС.НоменклатураПосле = Источник.ДокументОснование.Автомобиль;
				Иначе
					НС.Номенклатура = Источник.ДокументОснование.Автомобиль;
				КонецЕсли;
				НС.Количество = 1;
				НС.СчетУчета = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Источник.ДокументОснование.Ссылка,Справочники.Номенклатура.ПустаяСсылка()).СчетУчета;
				НС.СчетУчетаНУ = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Источник.ДокументОснование.Ссылка,Справочники.Номенклатура.ПустаяСсылка()).СчетУчета;
				НС.СчетУчетаНДС = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Источник.ДокументОснование.Ссылка,Справочники.Номенклатура.ПустаяСсылка()).СчетУчетаНДС;
				
				
				ИсходящиеДанные =Новый Структура;
				ИсходящиеДанные.Вставить("Товары",ТаблицаТовары);
				ИсходящиеДанные.Вставить("СИзмененеимНоменклатуры",СтараяСтатьяИзменениеВин или ХозОперацияИзменениеВин);
				ИмяПКО = "ПоступлениеДопРасходовАвтомобилиВСебестоимость";
			Иначе
				ИмяПКО = "ПоступлениеДопРасходовАвтомобилиНеВСебестоимость";
			КонецЕсли;
		КонецЕсли;
	КонецЕсли;
Иначе
	Отказ = Истина;
КонецЕсли;</ПередВыгрузкой>
