КонецЕсли;</Текст>
		</Алгоритм>
		<Алгоритм Имя="ЗарегистрироватьВПоследовательности" ИспользуетсяПриЗагрузке="true">
			<Текст>ПоследовательностиДокумента = РаботаСПоследовательностямиПовтИсп.ПолучитьСписокПоследовательностейДокумента(Док.Значение.Метаданные().Имя);
Для Каждого ПоследовательностьМетаданные Из ПоследовательностиДокумента Цикл
	// Получить набор записей последовательности				
	Набор = Док.Значение.ПринадлежностьПоследовательностям[ПоследовательностьМетаданные.Имя];
	Набор.Очистить();
	Если Док.Значение.Проведен Тогда // !
		Запрос = РаботаСПоследовательностямиПовтИсп.ПолучитьЗапросЗначенийИзмеренийПоследовательности(
		ПоследовательностьМетаданные.Имя, Док.Значение.Метаданные().Имя);
		
		Если Не Запрос = Неопределено Тогда
			Если ЗначениеЗаполнено(Запрос.Текст) Тогда
				Запрос.УстановитьПараметр("Ссылка", Док.Значение.Ссылка);
				ВсеЗначенияИзмерений = Запрос.Выполнить().Выгрузить();
				
				ВсеЗначенияИзмерений.Колонки.Добавить("Период", Новый ОписаниеТипов("Дата", , , Новый КвалификаторыДаты(ЧастиДаты.ДатаВремя)));
				ВсеЗначенияИзмерений.ЗаполнитьЗначения(Док.Значение.Дата, "Период");
				Набор.Загрузить(ВсеЗначенияИзмерений);
				// Подготовленный набор запишется автоматически, поскольку мы находимся в обработчике, а он взят из документа
				Набор.Записать();
			КонецЕсли;
		КонецЕсли;
	КонецЕсли;
	
КонецЦикла;</Текст>
		</Алгоритм>
		<Алгоритм Имя="НаценкаВнутреннихПерепродаж" ИспользуетсяПриЗагрузке="false">
			<Текст>//с наценкой


Свойство = ПланыВидовХарактеристик.СвойстваОбъектов.ДокументПерепродажи;
НаборЗаписейЗначенияСвойств = РегистрыСведений.ЗначенияСвойствОбъектов.СоздатьНаборЗаписей();
НаборЗаписейЗначенияСвойств.Отбор.Объект.Установить(Источник.Ссылка);
НаборЗаписейЗначенияСвойств.Отбор.Свойство.Установить(Свойство);
	
НаборЗаписейЗначенияСвойств.Прочитать();
Если НаборЗаписейЗначенияСвойств.Количество()&lt;&gt;0 Тогда
	Перепродажа = НаборЗаписейЗначенияСвойств[0].Значение;
Иначе
	Перепродажа = ложь;
КонецЕсли;


Если Не ТипЗнч(Источник.ДокументОснование) = Тип("ДокументСсылка.ЗаказНаряд") Тогда
	Если Перепродажа Тогда
		Если Найти(Источник.Организация.ИНН,"526209828675") &lt;&gt; 0  или Найти(Источник.Контрагент.ИНН,"526209828675") &lt;&gt; 0 Тогда//Поступления или Реализация на ИП Мусин
			Значение= Окр(Значение+Значение*Параметры.ПроцентНаценкиМусин/100,2);
		Иначе
			Значение= Окр(Значение+Значение*Параметры.ПроцентНаценки/100,2);
		КонецЕсли;
	Иначе
		СтрокаОрганизация = Источник.Организация.СвязаннаяИнформация.Найти(Источник.Контрагент,"СвязанныйКонтрагент");
		Если СтрокаОрганизация &lt;&gt; Неопределено Тогда
			Если Найти(СтрокаОрганизация.СвязанныйКонтрагент.ИНН,"526209828675") &lt;&gt; 0 ИЛИ Найти(Источник.Организация.ИНН,"526209828675") &lt;&gt; 0 Тогда//Поступления или Реализация на ИП Мусин
				Значение= Окр(Значение+Значение*Параметры.ПроцентНаценкиМусин/100,2);
			Иначе
				Значение= Окр(Значение+Значение*Параметры.ПроцентНаценки/100,2);
			КонецЕсли;
		КонецЕсли;
	КонецЕсли;
КонецЕсли;</Текст>
		</Алгоритм>
