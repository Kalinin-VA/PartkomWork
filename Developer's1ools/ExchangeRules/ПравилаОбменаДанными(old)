				<ПослеЗагрузки>ИДБазы = Параметры.ИДБазы;
СтрокаБазы = ПараметрыОбъекта.Получить("СтрокаСоединенияСБазой");
Родитель = Справочники.Номенклатура.НайтиПоРеквизиту("ИД",?(ЗначениеЗаполнено(ИДБазы),ИдБазы,СтрокаБазы));
Если Родитель.Пустая() Тогда
	НовРод = Справочники.Номенклатура.СоздатьГруппу();
	НовРод.Наименование = "Группа разделения по организации "+СтрокаБазы;
	НовРод.ИД = ?(ЗначениеЗаполнено(ИДБазы),ИдБазы,СтрокаБазы);
	НовРод.Записать();
	Родитель = НовРод.Ссылка;
КонецЕсли;


НайденныйОбъект = Справочники.Номенклатура.НайтиПоРеквизиту("ИД",СвойстваПоиска.Получить("ИД"),Родитель);
Если Не НайденныйОбъект.Пустая() Тогда
	Ссылка = НайденныйОбъект;
	Об = Ссылка.ПолучитьОбъект();
	Об.Родитель = Объект.Родитель;
КонецЕсли;

Если Объект.Родитель.Пустая() Тогда
	Объект.Родитель = Родитель;
КонецЕсли;</ПослеЗагрузки>
