	//Иначе
		Попытка
			Объект.ПодразделениеОрганизации = Справочники.ПодразделенияОрганизаций.ПустаяСсылка();
		Исключение
		КонецПопытки;
		ВремяДок = Формат(Объект.Дата, "ДФ=""гггг ММ дд   ЧЧ:мм:сс""");
		
		Если Объект.Проведен и Объект.Дата &gt;= Параметры.ДатаОграниченияГлубиныВыгрузки Тогда
			Параметры.СписокДокументовКПроведению.Добавить(Объект,ВремяДок);
		ИначеЕсли Объект.Дата &gt;= Параметры.ДатаОграниченияГлубиныВыгрузки Тогда
			Параметры.СписокДокументовНаУдаление.Добавить(Объект,ВремяДок);
		Иначе
			////Объект.Проведен = Ложь;
		КонецЕсли;
