				<Правило Отключить="false">
					<Код>ИзвлечениеТоваровИзПроизводства</Код>
					<Наименование>ИзвлечениеТоваровИзПроизводства</Наименование>
					<Порядок>500</Порядок>
					<КодПравилаКонвертации>ИзвлечениеТоваровИзПроизводства                   </КодПравилаКонвертации>
					<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
					<ОбъектВыборки>ДокументСсылка.ИзвлечениеТоваровИзПроизводства</ОбъектВыборки>
				</Правило>
				<Правило Отключить="false">
					<Код>ЗаказНаряд</Код>
					<Наименование>ЗаказНаряд</Наименование>
					<Порядок>550</Порядок>
					<КодПравилаКонвертации>ЗаказНаряд                                        </КодПравилаКонвертации>
					<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
					<ОбъектВыборки>ДокументСсылка.ЗаказНаряд</ОбъектВыборки>
					<ПередВыгрузкойОбъекта>ЭтоАСС = Объект.Организация.ИНН = "5262158548";

ТипКлиента = Объект.Контрагент.ТипКлиента;
ВидОтношений = Объект.Контрагент.ВидКонтрагента;

ТипКлиентаГК = (ТипКлиента = Справочники.ТипыКлиентов.ЮридическоеЛицоГК или ТипКлиента = Справочники.ТипыКлиентов.ЮридическоеЛицоГКАгатИПартнеры);
ВидОтношенийВнутренний = (ВидОтношений = Перечисления.ВидыКонтрагентов.ВнутреннееПодразделение);

КодУпр1 = Объект.ВидРемонта.КодУпр = 1; //ГАРАНТИЙНЫЙ РЕМОНТ
КодУпр2 = Объект.ВидРемонта.КодУпр = 2; //ГАРАНТИЙНЫЙ РЕМОНТ ДОБРОЙ ВОЛИ 
КодУпр3 = Объект.ВидРемонта.КодУпр = 3; //КОММЕРЧЕСКИЙ РЕМОНТ 
КодУпр4 = Объект.ВидРемонта.КодУпр = 4; //РЕМОНТ И ОБСЛУЖИВАНИЕ СОБСТВЕННОГО АВТОМОБИЛЯ 
КодУпр5 = Объект.ВидРемонта.КодУпр = 5; //РЕМОНТ ТОВАРНОГО АВТОМОБИЛЯ 
КодУпр6 = Объект.ВидРемонта.КодУпр = 6; //КУЗОВНОЙ РЕМОНТ СТОРОННИЙ 
КодУпр7 = Объект.ВидРемонта.КодУпр = 7; //КУЗОВНОЙ РЕМОНТ СОБСТВЕННЫЙ 
КодУпр8 = Объект.ВидРемонта.КодУпр = 8; //РЕКЛАМАЦИЯ 
КодУпр9 = Объект.ВидРемонта.КодУпр = 9; //ПРЕДПРОДАЖНОЕ СЕРВИСНОЕ ОБСЛУЖИВАНИЕ 
КодУпр10 = Объект.ВидРемонта.КодУпр = 10; //УСТАНОВКА ДОПОЛНИТЕЛЬНОГО ОБОРУДОВАНИЯ ЗА СЧЕТ КЛИЕНТА 
КодУпр11 = Объект.ВидРемонта.КодУпр = 11; //УСТАНОВКА ДОПОЛНИТЕЛЬНОГО ОБОРУДОВАНИЯ ЗА СЧЕТ САЛОНА
КодУпр12 = Объект.ВидРемонта.КодУпр = 12; //УСТАНОВКА ДОПОЛНИТЕЛЬНОГО ОБОРУДОВАНИЯ В СТОИМОСТИ АВТОМОБИЛЯ 
КодУпр13 = Объект.ВидРемонта.КодУпр = 13; //ЛЮКС ТОВАРНОГО АВТОМОБИЛЯ ВНУТРЕННИЙ
КодУпр14 = Объект.ВидРемонта.КодУпр = 14; //ТЕХНИЧЕСКОЕ ОБСЛУЖИВАНИЕ 
КодУпр15 = Объект.ВидРемонта.КодУпр = 15; //ТЕХНИЧЕСКОЕ ОБСЛУЖИВАНИЕ НЕВОЗМЕЩАЕМОЕ 

Отказ = КодУпр13;
НужноРазбитьРеализацию = Ложь;
Если КодУпр1 Тогда
	
	Если Не ТипКлиентаГК Тогда
		НеНужнаРеализация = Истина;
	Иначе
		НеНужнаРеализация = Ложь;
	КонецЕсли;
	Если ЭтоАСС Тогда
		НеНужнаРеализация = Ложь;
		НужноРазбитьРеализацию = Ложь;
	КонецЕсли;
	
ИначеЕсли КодУпр2 ИЛИ КодУпр8 ИЛИ КодУпр15 Тогда
	
	НеНужнаРеализация = Истина;
	
ИначеЕсли КодУпр4 ИЛИ КодУпр5 ИЛИ КодУпр9 ИЛИ КодУпр11 ИЛИ КодУпр12 ИЛИ КодУпр10 Тогда //Бред, но по заявке 91142 при 10-м ремонте делать выручку не надо...
	Если ТипКлиентаГК и ВидОтношенийВнутренний Тогда
		НеНужнаРеализация = Истина;
	Иначе
		НеНужнаРеализация = Ложь;
	КонецЕсли;
	Если ЭтоАСС Тогда
		НужноРазбитьРеализацию = Истина;
	Иначе
		НужноРазбитьРеализацию = Ложь;
	КонецЕсли;

Иначе
	
	НеНужнаРеализация = Ложь;
	Если ЭтоАСС Тогда
		НужноРазбитьРеализацию = Истина;
	КонецЕсли;
		
	
КонецЕсли;
		
//НеНужнаРеализация = Истина;

Если не Отказ Тогда
	Если НЕ ЭтоАСС Тогда
		Запрос=новый Запрос;
		Запрос.Текст="ВЫБРАТЬ
					|	ТоварыВПроизводствеОбороты.Номенклатура,
					|	ТоварыВПроизводствеОбороты.КоличествоРасход КАК Количество,
					|	ТоварыВПроизводствеОбороты.Номенклатура.БазоваяЕдиницаИзмерения КАК ЕдиницаИзмерения 
					|ИЗ
					|	РегистрНакопления.ТоварыВПроизводстве.Обороты(, , Регистратор,  #УсловиеЗН ) КАК ТоварыВПроизводствеОбороты
					|ГДЕ
					|	ТоварыВПроизводствеОбороты.Регистратор = &amp;Ссылка
					|	И НЕ ТоварыВПроизводствеОбороты.Номенклатура.ТипНоменклатурыУправленческий = Значение(Справочник.ТипыНоменклатурыУправленческий.СторонниеУслугиИРаботы)
					|	И НЕ ТоварыВПроизводствеОбороты.Номенклатура.ЕдиныйКлассификаторНоменклатурыАвтозапчастей = &amp;ЗначениеРМНормируемых";
		Запрос.УстановитьПараметр("Ссылка",Объект.Ссылка);
		Попытка 
			Запрос.УстановитьПараметр("ЗначениеРМНормируемых",Справочники.ЕдиныйКлассификаторНоменклатурыАвтозапчастей.НормируемыеРМ);	
		Исключение
			Запрос.УстановитьПараметр("ЗначениеРМНормируемых",Справочники.ЕдиныйКлассификаторНоменклатурыАвтозапчастей.ПустаяСсылка());
		КонецПопытки;
		Если Объект.ВидРемонта = Справочники.ВидыРемонта.ЛюксТоварногоАвтомобиляКлиента Тогда
			Запрос.Текст = СтрЗаменить(Запрос.Текст, "#УсловиеЗН", "");
		Иначе
			Запрос.Текст = СтрЗаменить(Запрос.Текст, "#УсловиеЗН", "ЗаказНаряд = &amp;Ссылка");
		КонецЕсли;
			
		ТаблицаТовары=Новый ТаблицаЗначений;
		ЗаказНаряд=Неопределено;
		ТаблицаТовары.Колонки.Добавить("Номенклатура",Новый ОписаниеТипов("СправочникСсылка.Номенклатура"));
		ТаблицаТовары.Колонки.Добавить("Количество",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаТовары.Колонки.Добавить("Счет");
		ТаблицаТовары.Колонки.Добавить("ЕдиницаИзмерения");

		РезультатЗапроса=Запрос.Выполнить();
		ВыборкаЗН=РезультатЗапроса.Выбрать();
		Пока ВыборкаЗН.Следующий() Цикл
			НС = ТаблицаТовары.Добавить();
			НС.Номенклатура = ВыборкаЗН.Номенклатура;
			НС.Количество = ВыборкаЗН.Количество;
			НС.Счет = ПланыСчетов.Хозрасчетный.Пустаяссылка();//БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,ВыборкаЗН.Номенклатура);
			НС.ЕдиницаИзмерения = ВыборкаЗН.ЕдиницаИзмерения
		КонецЦикла;
		
		Если ТаблицаТовары.Количество() &lt;&gt; 0 Тогда
			ВходящийДанные=Новый Структура("Материалы",ТаблицаТовары);
			ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящийДанные,,"ТребованиеНакладная");
			Если НЕ НеНужнаРеализация Тогда
				ВыгрузитьПоПравилу(Объект.Ссылка,,,,"ЗаказНаряд");
			КонецЕсли;
		Иначе
			Если НЕ НеНужнаРеализация Тогда
				ВыгрузитьПоПравилу(Объект.Ссылка,,,,"ЗаказНаряд");
			КонецЕсли;
		КонецЕсли;
	Иначе //Скания
		Запрос=новый Запрос;
		Запрос.Текст="ВЫБРАТЬ
		             |	ЗаказНарядТовары.Ссылка,
		             |	ЗаказНарядТовары.Номенклатура,
					 |	ЗаказНарядТовары.Номенклатура.БазоваяЕдиницаИзмерения КАК ЕдиницаИзмерения,
		             |	ЗаказНарядТовары.Количество,
		             |	ЗаказНарядТовары.ЕдиницаИзмерения,
		             |	ЗаказНарядТовары.Коэффициент,
		             |	ЗаказНарядТовары.Цена,
		             |	ЗаказНарядТовары.Сумма,
		             |	ЗаказНарядТовары.СтавкаНДС,
		             |	ЗаказНарядТовары.СуммаНДС,
		             |	ЗаказНарядТовары.СуммаВсего
		             |ИЗ
		             |	Документ.ЗаказНаряд.Товары КАК ЗаказНарядТовары
		             |ГДЕ
		             |	ЗаказНарядТовары.Ссылка = &amp;Ссылка
					 |	И НЕ ЗаказНарядТовары.Номенклатура.ТипНоменклатурыУправленческий = Значение(Справочник.ТипыНоменклатурыУправленческий.СторонниеУслугиИРаботы)
					 |	и Не ЗаказНарядТовары.Источник= Значение(Перечисление.ЗаказНарядНоменклатураИсточник.ДетальЗаказчика)
					 |	И НЕ ЗаказНарядТовары.Номенклатура.ЕдиныйКлассификаторНоменклатурыАвтозапчастей = &amp;ЗначениеРМНормируемых";
		Запрос.УстановитьПараметр("Ссылка",Объект.Ссылка);
		Попытка 
			Запрос.УстановитьПараметр("ЗначениеРМНормируемых",Справочники.ЕдиныйКлассификаторНоменклатурыАвтозапчастей.НормируемыеРМ);	
		Исключение
			Запрос.УстановитьПараметр("ЗначениеРМНормируемых",Справочники.ЕдиныйКлассификаторНоменклатурыАвтозапчастей.ПустаяСсылка());
		КонецПопытки;
		ТаблицаТовары=Новый ТаблицаЗначений;
		ЗаказНаряд=Неопределено;
		ТаблицаТовары.Колонки.Добавить("Номенклатура",Новый ОписаниеТипов("СправочникСсылка.Номенклатура"));
		ТаблицаТовары.Колонки.Добавить("ЕдиницаИзмерения");
		ТаблицаТовары.Колонки.Добавить("Коэффициент");
		ТаблицаТовары.Колонки.Добавить("СтавкаНДС");
		ТаблицаТовары.Колонки.Добавить("Количество",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаТовары.Колонки.Добавить("Цена",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаТовары.Колонки.Добавить("СуммаНДС",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаТовары.Колонки.Добавить("Сумма",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаТовары.Колонки.Добавить("Субконто");
		ТаблицаТовары.Колонки.Добавить("СчетУчета");
		ТаблицаТовары.Колонки.Добавить("Счет");
		ТаблицаТовары.Колонки.Добавить("СчетУчетаНДСПоРеализации");
		ТаблицаТовары.Колонки.Добавить("СчетДоходов");
		ТаблицаТовары.Колонки.Добавить("СчетРасходов");
		ПустаяТаблицаТовары = ТаблицаТовары.Скопировать();

		РезультатЗапроса=Запрос.Выполнить();
		ВыборкаЗН=РезультатЗапроса.Выбрать();
		Пока ВыборкаЗН.Следующий() Цикл
			НС = ТаблицаТовары.Добавить();
			НС.Номенклатура = ВыборкаЗН.Номенклатура;
			НС.ЕдиницаИзмерения = ВыборкаЗН.ЕдиницаИзмерения;
			НС.Коэффициент = ВыборкаЗН.Коэффициент;
			НС.СтавкаНДС = ВыборкаЗН.СтавкаНДС;
			НС.Количество = ВыборкаЗН.Количество;
			НС.Цена = ВыборкаЗН.Цена;
			НС.СуммаНДС = ВыборкаЗН.СуммаНДС;
			НС.Сумма = ВыборкаЗН.СуммаВсего;
			НС.Субконто = ВыборкаЗН.Номенклатура.Бренд;
			НС.СчетУчета = ПланыСчетов.Регламентированный.СырьеИМатериалы;//БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,ВыборкаЗН.Номенклатура).СчетУчета;
			НС.СчетУчетаНДСПоРеализации = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,ВыборкаЗН.Номенклатура).СчетУчетаНДСПродажи;
			Попытка
				НС.СчетДоходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,ВыборкаЗН.Номенклатура,,ВыборкаЗН.СтавкаНДС).СчетДоходов;
			Исключение
				НС.СчетДоходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,ВыборкаЗН.Номенклатура).СчетДоходов;
			КонецПопытки;
			Попытка
				НС.СчетРасходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,ВыборкаЗН.Номенклатура,,ВыборкаЗН.СтавкаНДС).СчетРасходов;
			Исключение
				НС.СчетРасходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,ВыборкаЗН.Номенклатура).СчетРасходов;
			КонецПопытки;
			НС.Счет = ПланыСчетов.Хозрасчетный.Пустаяссылка();
		КонецЦикла;
		
		//расходка по мойке не указывается в табличной части ЗН
		Запрос=новый Запрос;
		Запрос.Текст="ВЫБРАТЬ
					|	ТоварыВПроизводствеОбороты.Номенклатура,
					|	ТоварыВПроизводствеОбороты.КоличествоРасход КАК Количество,
					|	ТоварыВПроизводствеОбороты.Номенклатура.БазоваяЕдиницаИзмерения КАК ЕдиницаИзмерения 
					|ИЗ
					|	РегистрНакопления.ТоварыВПроизводстве.Обороты(, , Регистратор,  #УсловиеЗН ) КАК ТоварыВПроизводствеОбороты
					|ГДЕ
					|	ТоварыВПроизводствеОбороты.Регистратор = &amp;Ссылка
					|	И НЕ ТоварыВПроизводствеОбороты.Номенклатура.ТипНоменклатурыУправленческий = Значение(Справочник.ТипыНоменклатурыУправленческий.СторонниеУслугиИРаботы)
					|	И НЕ ТоварыВПроизводствеОбороты.Номенклатура.ЕдиныйКлассификаторНоменклатурыАвтозапчастей = &amp;ЗначениеРМНормируемых";
		Запрос.УстановитьПараметр("Ссылка",Объект.Ссылка);
		Попытка 
			Запрос.УстановитьПараметр("ЗначениеРМНормируемых",Справочники.ЕдиныйКлассификаторНоменклатурыАвтозапчастей.НормируемыеРМ);	
		Исключение
			Запрос.УстановитьПараметр("ЗначениеРМНормируемых",Справочники.ЕдиныйКлассификаторНоменклатурыАвтозапчастей.ПустаяСсылка());
		КонецПопытки;
		Если Объект.ВидРемонта = Справочники.ВидыРемонта.ЛюксТоварногоАвтомобиляКлиента Тогда
			Запрос.Текст = СтрЗаменить(Запрос.Текст, "#УсловиеЗН", "");
		Иначе
			Запрос.Текст = СтрЗаменить(Запрос.Текст, "#УсловиеЗН", "ЗаказНаряд = &amp;Ссылка");
		КонецЕсли;
			
		ТаблицаТоварыМойки=Новый ТаблицаЗначений;
		ЗаказНаряд=Неопределено;
		ТаблицаТоварыМойки.Колонки.Добавить("Номенклатура",Новый ОписаниеТипов("СправочникСсылка.Номенклатура"));
		ТаблицаТоварыМойки.Колонки.Добавить("Количество",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаТоварыМойки.Колонки.Добавить("Счет",Новый ОписаниеТипов("ПланСчетовСсылка.Регламентированный"));
		ТаблицаТоварыМойки.Колонки.Добавить("ЕдиницаИзмерения",Новый ОписаниеТипов("СправочникСсылка.ЕдиницыИзмерения"));

		РезультатЗапроса=Запрос.Выполнить();
		ВыборкаЗН=РезультатЗапроса.Выбрать();
		Пока ВыборкаЗН.Следующий() Цикл
			НС = ТаблицаТоварыМойки.Добавить();
			НС.Номенклатура = ВыборкаЗН.Номенклатура;
			НС.Количество = ВыборкаЗН.Количество;
			НС.Счет = ПланыСчетов.Хозрасчетный.Пустаяссылка();
			НС.ЕдиницаИзмерения = ВыборкаЗН.ЕдиницаИзмерения
		КонецЦикла;
        Для каждого стр из ТаблицаТовары Цикл
			НС = ТаблицаТоварыМойки.Добавить();
			НС.Номенклатура = стр.Номенклатура;
			НС.Количество = - стр.Количество;
			НС.Счет = ПланыСчетов.Хозрасчетный.Пустаяссылка();
			НС.ЕдиницаИзмерения = стр.ЕдиницаИзмерения;
		КонецЦикла;
		
		ТаблицаТоварыМойки.Свернуть("Номенклатура,Счет,ЕдиницаИзмерения","Количество");
		
		
		Для каждого стр из ТаблицаТоварыМойки Цикл
			Если Не ЗначениеЗаполнено(Стр.Количество) Тогда
				ТаблицаТоварыМойки.Удалить(Стр);
			КонецЕсли;
		КонецЦикла;
		

		
		
		Запрос=новый Запрос;
		Запрос.Текст="ВЫБРАТЬ
		             |	ЗаказНарядТовары.Ссылка,
		             |	ЗаказНарядТовары.Номенклатура,
					 |	ЗаказНарядТовары.Количество,
		             |	ЗаказНарядТовары.ЕдиницаИзмерения,
		             |	ЗаказНарядТовары.Цена,
		             |	ЗаказНарядТовары.Сумма,
		             |	ЗаказНарядТовары.СтавкаНДС,
		             |	ЗаказНарядТовары.СуммаНДС,
		             |	ЗаказНарядТовары.СуммаВсего
		             |ИЗ
		             |	Документ.ЗаказНаряд.Товары КАК ЗаказНарядТовары
		             |ГДЕ
		             |	ЗаказНарядТовары.Ссылка = &amp;Ссылка
					 |	И (ЗаказНарядТовары.Номенклатура.ТипНоменклатурыУправленческий = Значение(Справочник.ТипыНоменклатурыУправленческий.СторонниеУслугиИРаботы)
					 |     Или  ЗаказНарядТовары.Номенклатура.ЕдиныйКлассификаторНоменклатурыАвтозапчастей = &amp;ЗначениеРМНормируемых)";
		Запрос.УстановитьПараметр("Ссылка",Объект.Ссылка);
		Попытка 
			Запрос.УстановитьПараметр("ЗначениеРМНормируемых",Справочники.ЕдиныйКлассификаторНоменклатурыАвтозапчастей.НормируемыеРМ);	
		Исключение
			Запрос.УстановитьПараметр("ЗначениеРМНормируемых",Справочники.ЕдиныйКлассификаторНоменклатурыАвтозапчастей.ПустаяСсылка());
		КонецПопытки;
		
		ТаблицаУслуги=Новый ТаблицаЗначений;
		ЗаказНаряд=Неопределено;
		ТаблицаУслуги.Колонки.Добавить("Номенклатура",Новый ОписаниеТипов("СправочникСсылка.Номенклатура"));
		ТаблицаУслуги.Колонки.Добавить("СтавкаНДС");
		ТаблицаУслуги.Колонки.Добавить("Количество",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаУслуги.Колонки.Добавить("Цена",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаУслуги.Колонки.Добавить("СуммаНДС",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаУслуги.Колонки.Добавить("Сумма",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаУслуги.Колонки.Добавить("Субконто");
		ТаблицаУслуги.Колонки.Добавить("Содержание");
		ТаблицаУслуги.Колонки.Добавить("СчетУчетаНДСПоРеализации");
		ТаблицаУслуги.Колонки.Добавить("СчетДоходов");
		ТаблицаУслуги.Колонки.Добавить("СчетРасходов");
		ПустаяТаблицаУслуги = ТаблицаУслуги.Скопировать();
		
		НС = ТаблицаУслуги.Добавить();
		НС.Номенклатура = Справочники.Номенклатура.Авторабота;
		НС.СтавкаНДС = Справочники.СтавкиНДС.БезНДС;
		НС.Количество = 1;
		НС.СуммаНДС = Объект.Работы.Итог("СуммаНДС");
		НС.Сумма = Объект.Работы.Итог("СуммаВсего");
		НС.Цена = ?(НС.Сумма = 0, 0, НС.Сумма/НС.Количество);
		НС.СчетУчетаНДСПоРеализации = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,НС.Номенклатура).СчетУчетаНДСПродажи;
		Попытка
			НС.СчетДоходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,НС.Номенклатура,,НС.СтавкаНДС).СчетДоходов;
		Исключение
			НС.СчетДоходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,НС.Номенклатура).СчетДоходов;
		КонецПопытки;
		Попытка
			НС.СчетРасходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,НС.Номенклатура,,НС.СтавкаНДС).СчетРасходов;
		Исключение
			НС.СчетРасходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,НС.Номенклатура).СчетРасходов;
		КонецПопытки;
		НС.Содержание = НС.Номенклатура.НаименованиеПолное;
				
		РезультатЗапроса=Запрос.Выполнить();
		ВыборкаЗН=РезультатЗапроса.Выбрать();
		Пока ВыборкаЗН.Следующий() Цикл
			НС = ТаблицаУслуги.Добавить();
			НС.Номенклатура = ВыборкаЗН.Номенклатура;
			НС.СтавкаНДС = ВыборкаЗН.СтавкаНДС;
			НС.Количество = ВыборкаЗН.Количество;
			НС.Цена = ВыборкаЗН.Цена;
			НС.СуммаНДС = ВыборкаЗН.СуммаНДС;
			НС.Сумма = ВыборкаЗН.СуммаВсего;
			НС.Субконто = ВыборкаЗН.Номенклатура.Бренд;
			НС.СчетУчетаНДСПоРеализации = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,ВыборкаЗН.Номенклатура).СчетУчетаНДСПродажи;
			Попытка
				НС.СчетДоходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,ВыборкаЗН.Номенклатура,,ВыборкаЗН.СтавкаНДС).СчетДоходов;
			Исключение
				НС.СчетДоходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,ВыборкаЗН.Номенклатура).СчетДоходов;
			КонецПопытки;
			Попытка
				НС.СчетРасходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,ВыборкаЗН.Номенклатура,,ВыборкаЗН.СтавкаНДС).СчетРасходов;
			Исключение
				НС.СчетРасходов = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,ВыборкаЗН.Номенклатура).СчетРасходов;
			КонецПопытки;
			НС.Содержание = ВыборкаЗН.Номенклатура.НаименованиеПолное;
		КонецЦикла;
		
		
		Если НеНужнаРеализация Тогда
			Если ТаблицаТовары.Количество() &lt;&gt; 0 Тогда
				ВходящийДанные=Новый Структура("Материалы",ТаблицаТовары);
				ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящийДанные,,"ТребованиеНакладная");
			КонецЕсли;
		Иначе
			Если НужноРазбитьРеализацию Тогда
				Если ТаблицаТовары.Количество() &gt; 0 Тогда
					ВходящийДанные=Новый Структура;
					ВходящийДанные.Вставить("Товары",ТаблицаТовары);
					ВходящийДанные.Вставить("Услуги",ПустаяТаблицаУслуги);
					ВходящийДанные.Вставить("Склад","НезавершенноеПроизводство");
					ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящийДанные,,"ЗаказНарядАСС");
				КонецЕсли;
				ВходящийДанныеУслуги=Новый Структура;
				ВходящийДанныеУслуги.Вставить("Товары",ПустаяТаблицаТовары);
				ВходящийДанныеУслуги.Вставить("Услуги",ТаблицаУслуги);
				ВходящийДанныеУслуги.Вставить("Склад",Справочники.СкладыКомпании.ПустаяСсылка());
				ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящийДанныеУслуги,,"ЗаказНарядАСС");
			Иначе
				Если ТаблицаТовары.Количество() &lt;&gt; 0 Тогда
					ВходящийДанные=Новый Структура("Материалы",ТаблицаТовары);
					ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящийДанные,,"ТребованиеНакладная");
				КонецЕсли;
				ВыгрузитьПоПравилу(Объект.Ссылка,,,,"ЗаказНаряд");	
			КонецЕсли;
		КонецЕсли;
		Если ТаблицаТоварыМойки.Количество() &lt;&gt; 0 Тогда
			ВходящийДанные=Новый Структура("Материалы",ТаблицаТоварыМойки);
			ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящийДанные,,"ТребованиеНакладная");
		КонецЕсли;
		
		
	КонецЕсли;
КонецЕсли;

Отказ = Истина;</ПередВыгрузкойОбъекта>
				</Правило>
				<Правило Отключить="false">
					<Код>СписатьРасходныеМатериалы</Код>
					<Наименование>Списать расходные материалы</Наименование>
					<Порядок>600</Порядок>
					<КодПравилаКонвертации>СписатьРасходныеМатериалы                         </КодПравилаКонвертации>
					<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
					<ОбъектВыборки>ДокументСсылка.СписатьРасходныеМатериалы</ОбъектВыборки>
				</Правило>
			</Группа>
			<Группа Отключить="false">
				<Код>ДокументыПоАвтомобилям</Код>
				<Наименование>ДокументыПоАвтомобилям</Наименование>
				<Порядок>250</Порядок>
				<Группа Отключить="false">
					<Код>ПоступленияАвтомобилей</Код>
					<Наименование>ПоступленияАвтомобилей</Наименование>
					<Порядок>50</Порядок>
					<Правило Отключить="false">
						<Код>ПоступлениеАвтомобилей</Код>
						<Наименование>Поступление автомобилей</Наименование>
						<Порядок>100</Порядок>
						<КодПравилаКонвертации>ПоступлениеАвтомобилей                            </КодПравилаКонвертации>
						<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
						<ОбъектВыборки>ДокументСсылка.ПоступлениеАвтомобилей</ОбъектВыборки>
						<ПередВыгрузкойОбъекта>Если Объект.ХозОперация = Справочники.ХозОперации.ПоступлениеАвтомобилейОтветственноеХранение Тогда
	ВыгрузитьПоПравилу(Объект.Ссылка, , , , "ПоступлениеАвтомобилей");
ИначеЕсли Объект.ХозОперация = Справочники.ХозОперации.ПереходВСобственностьАвтомобилей Тогда
	ВыгрузитьПоПравилу(Объект.Ссылка, , , , "ВозвратАвтомобиляИзХранения");
	ВыгрузитьПоПравилу(Объект.Ссылка, , , , "ПоступлениеАвтомобилей");
Иначе
	ВыгрузитьПоПравилу(Объект.Ссылка, , , , "ПоступлениеАвтомобилей");
КонецЕсли;

