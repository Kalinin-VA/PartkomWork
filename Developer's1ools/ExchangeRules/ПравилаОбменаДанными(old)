				<ПослеВыгрузки>Если ЗначениеЗаполнено(Источник.Сотрудник) Тогда
	Сотрудник = Новый Структура;
	Сотрудник.Вставить("ФизическоеЛицо",Источник.Ссылка);
	
	ВыгрузитьПоПравилу(Источник.Сотрудник, ,Сотрудник, , "Сотрудники");
	
	КадровыеДанные = Новый Структура;
	КадровыеДанные.Вставить("Сотрудник",Источник.Сотрудник);
	КадровыеДанные.Вставить("ФизическоеЛицо",Источник.Ссылка);
	КадровыеДанные.Вставить("ГоловнаяОрганизация",Источник.Сотрудник.Организация);
	КадровыеДанные.Вставить("ТекущаяОрганизация",Источник.Сотрудник.Организация);
	КадровыеДанные.Вставить("ТекущееПодразделение",Источник.Сотрудник.Подразделение);
	КадровыеДанные.Вставить("ТекущаяДолжность",Источник.Сотрудник.Должность);
	КадровыеДанные.Вставить("ДатаПриема",?(ЗначениеЗаполнено(Источник.Сотрудник.ДатаПриема),Источник.Сотрудник.ДатаПриема,'19800101'));
	КадровыеДанные.Вставить("Период",?(ЗначениеЗаполнено(Источник.Сотрудник.ДатаПриема),Источник.Сотрудник.ДатаПриема,'19800101'));
	КадровыеДанные.Вставить("ОсновноеРабочееМестоВОрганизации",Истина);
	КадровыеДанные.Вставить("ТекущийВидЗанятости","ОсновноеМестоРаботы");
	
	
	ВыгрузитьПоПравилу(, ,КадровыеДанные, , "ТекущиеКадровыеДанныеСотрудников");
КонецЕсли;</ПослеВыгрузки>
				<ПослеЗагрузки>Попытка
	ТаблицаСрезПоследних = РегистрыСведений.ФИОФизическихЛиц.СрезПоследних(, Новый Структура("ФизическоеЛицо", Объект.Ссылка));
	Если ТаблицаСрезПоследних.Количество() = 0 Тогда
		
		МенеджерЗаписиФИО = РегистрыСведений.ФИОФизическихЛиц.СоздатьМенеджерЗаписи();
		
		СтруктураФИО = ФизическиеЛицаКлиентСервер.ФамилияИмяОтчество(Объект.ФИО);

		МенеджерЗаписиФИО.ФизическоеЛицо = Объект.Ссылка;
		МенеджерЗаписиФИО.Фамилия = ?(СтруктураФИО.Фамилия = Неопределено, "", СтруктураФИО.Фамилия);
		МенеджерЗаписиФИО.Имя = ?(СтруктураФИО.Имя = Неопределено, "", СтруктураФИО.Имя);
		МенеджерЗаписиФИО.Отчество = ?(СтруктураФИО.Отчество = Неопределено, "", СтруктураФИО.Отчество);
		МенеджерЗаписиФИО.Период = '19800101';
		
		МенеджерЗаписиФИО.Записать();
		
	КонецЕсли;
Исключение
