		ТаблицаТовары.Колонки.Добавить("ЕдиницаИзмерения");
		ТаблицаТовары.Колонки.Добавить("Коэффициент");
		ТаблицаТовары.Колонки.Добавить("СтавкаНДС");
		ТаблицаТовары.Колонки.Добавить("Количество",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаТовары.Колонки.Добавить("Цена",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаТовары.Колонки.Добавить("СуммаНДС",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаТовары.Колонки.Добавить("Сумма",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаТовары.Колонки.Добавить("Субконто");
		ТаблицаТовары.Колонки.Добавить("СчетУчета");
		ТаблицаТовары.Колонки.Добавить("Счет");
		ТаблицаТовары.Колонки.Добавить("СчетУчетаНДСПоРеализации");
		ТаблицаТовары.Колонки.Добавить("СчетДоходов");
		ТаблицаТовары.Колонки.Добавить("СчетРасходов");
		ПустаяТаблицаТовары = ТаблицаТовары.Скопировать();

