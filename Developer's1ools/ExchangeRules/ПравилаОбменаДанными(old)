			<Текст>ВыбПольз = Справочники.Пользователи.Выбрать();
ВыбПольз.Следующий();
Польз = ВыбПольз.Ссылка;

Запрос=новый Запрос;

Запрос.Текст="ВЫБРАТЬ
|	АвизоСостав.Ссылка Как Ссылка,
|	АвизоСостав.Автомобиль Как Автомобиль,
|   АвизоСостав.СуммаПриход КАк СуммаПриход
|ИЗ
|	Документ.Авизо.Состав КАК АвизоСостав
|ГДЕ
|	АвизоСостав.Ссылка = &amp;Ссылка";
Запрос.УстановитьПараметр("Ссылка",Объект.Ссылка);
ТаблицаТовары=Новый ТаблицаЗначений;

ТаблицаТовары.Колонки.Добавить("Номенклатура",	Новый ОписаниеТипов("СправочникСсылка.Автомобили"));
ТаблицаТовары.Колонки.Добавить("Сумма",			Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
РезультатЗапроса=Запрос.Выполнить();
ВыборкаНомерВх=РезультатЗапроса.Выбрать();
ш = 1;
Пока ВыборкаНомерВх.Следующий() Цикл
	Сообщить(ВыборкаНомерВх.Ссылка);
	Сообщить(ВыборкаНомерВх.Автомобиль);
	Сообщить(ВыборкаНомерВх.Ссылка.Дата);

	ВходящиеДанные = Новый Структура;
	ВходящиеДанные.Вставить("Дата",	Дата(ВыборкаНомерВх.Ссылка.Дата));
	ВходящиеДанные.Вставить("Организация", ВыборкаНомерВх.Ссылка.Организация);
	ВходящиеДанные.Вставить("Комментарий", Строка(ВыборкаНомерВх.Ссылка.Комментарий));
	ВходящиеДанные.Вставить("Проведен", ВыборкаНомерВх.Ссылка.Проведен);
	ТаблицаТовары.Очистить();
	СтрокаТаблицыТовары = ТаблицаТовары.Добавить();
	ВходящиеДанные.Вставить("Номер",	ВыборкаНомерВх.Ссылка.Номер);
	ш=ш+1;
	СтрокаТаблицыТовары.Номенклатура	= ВыборкаНомерВх.Автомобиль;
	СтрокаТаблицыТовары.Сумма = ВыборкаНомерВх.СуммаПриход;
	ВходящиеДанные.Вставить("ВалютаДокумента", ВыборкаНомерВх.Ссылка.ВалютаДокумента);
	ВходящиеДанные.Вставить("Ответственный",Польз);
	ВходящиеДанные.Вставить("Организация",ВыборкаНомерВх.Ссылка.Организация);
	ВходящиеДанные.Вставить("Товары",ТаблицаТовары);
	ВыгрузитьПоПравилу(ВыборкаНомерВх.Ссылка,,ВходящиеДанные ,, "АвизоКорректировкаПоступления");
КонецЦикла;</Текст>
		</Алгоритм>
		<Алгоритм Имя="ВыгрузитьДопРасходыАвто" ИспользуетсяПриЗагрузке="false">
			<Текст>ЗапросАвто=новый Запрос;
ЗапросАвто.Текст="
|ВЫБРАТЬ
|	ОстаткиАвтомобилейОстатки.КоличествоОстаток Как Колво
|ИЗ
|	РегистрНакопления.ОстаткиАвтомобилей.Остатки(&amp;ДатаНач, Автомобиль В (&amp;Автомобиль)) КАК ОстаткиАвтомобилейОстатки
|ГДЕ
|	ОстаткиАвтомобилейОстатки.КоличествоОстаток &gt; 0";
НаМомент = Новый Граница(Объект.Дата,ВидГраницы.Включая);
ЗапросАвто.УстановитьПараметр("ДатаНач",НаМомент);
ЗапросАвто.УстановитьПараметр("Автомобиль",Объект.Автомобиль);
Если ЗапросАвто.Выполнить().Пустой() Тогда
	АвтомобильНаОстатках = Ложь;
Иначе
	АвтомобильНаОстатках = Истина;
КонецЕсли;

// Исключение пока убрано, по заявке 66762
//ИсключениеГрузитьНеНадо = СОКРЛП(Объект.СтатьяДоходовИРасходовДляОбмена.НовыйКод) =  "05.01.14" //Статья Предпродажное сервисное обслуживание автомобилей
//							ИЛИ 
							
ИсключениеГрузитьНеНадо = Найти(Объект.Контрагент.ИНН,"7706640206")&gt;0; // Контрагент РАТ
							
ИсключениеГрузитьНеНадо = ИсключениеГрузитьНеНадо ИЛИ СОКРЛП(Объект.СтатьяДоходовИРасходовДляОбмена.Код2014) =  "07.22.10";//Статья ДиР – Банковские услуги


СтараяСтатьяИзменениеВин = СОКРЛП(Объект.СтатьяДоходовИРасходовДляОбмена.Код) =  "41.01.0116";
ХозОперацияИзменениеВин = Объект.ХозОперация = Справочники.ХозОперации.ПереоборудованиеСИзменениемVIN;
СтатьяНаСебестоимость = //СОКРЛП(Объект.СтатьяДоходовИРасходовДляОбмена.НовыйКод) = "05.01.12" //Транспортировка и доставка автомобилей
						//ИЛИ  Пока исключили
						СОКРЛП(Объект.СтатьяДоходовИРасходовДляОбмена.Код2014) = "05.01.13" //Расходы на таможню по автомобилям
						ИЛИ СОКРЛП(Объект.СтатьяДоходовИРасходовДляОбмена.Код2014) = "05.01.15" //Переоборудование автомобилей
						//Добавлены статьи по заявке №120701
						ИЛИ СОКРЛП(Объект.СтатьяДоходовИРасходовДляОбмена.Код2014) = "05.01.12" //Транспортировка и доставка автомобилей до склада
						ИЛИ СОКРЛП(Объект.СтатьяДоходовИРасходовДляОбмена.Код2014) = "05.01.16" //Дополнительное оборудование на автомобили
						ИЛИ СОКРЛП(Объект.СтатьяДоходовИРасходовДляОбмена.Код2014) = "07.25.10"; //Изменение себестоимости товарного автомобиля
						
						
Если ИсключениеГрузитьНеНадо Тогда
	Отказ = Истина;
Иначе
	Если (СтараяСтатьяИзменениеВин или ХозОперацияИзменениеВин или СтатьяНаСебестоимость) и АвтомобильНаОстатках Тогда
								
		
		ТаблицаТовары=Новый ТаблицаЗначений;
		ТаблицаТовары.Колонки.Добавить("Номенклатура",Новый ОписаниеТипов("СправочникСсылка.Автомобили"));
		ТаблицаТовары.Колонки.Добавить("НоменклатураПосле",Новый ОписаниеТипов("СправочникСсылка.Автомобили"));
		ТаблицаТовары.Колонки.Добавить("Количество",Новый ОписаниеТипов("Число",Новый КвалификаторыЧисла(15,2)));
		ТаблицаТовары.Колонки.Добавить("СчетУчета",Новый ОписаниеТипов("ПланСчетовСсылка.Регламентированный"));
		ТаблицаТовары.Колонки.Добавить("СчетУчетаНУ",Новый ОписаниеТипов("ПланСчетовСсылка.Регламентированный"));
		ТаблицаТовары.Колонки.Добавить("СчетУчетаНДС",Новый ОписаниеТипов("ПланСчетовСсылка.Регламентированный"));
		
		НС = ТаблицаТовары.Добавить();
		Если СтараяСтатьяИзменениеВин или ХозОперацияИзменениеВин Тогда
			НС.Номенклатура = Объект.АвтомобильДоДоработки;
			НС.НоменклатураПосле = Объект.Автомобиль;
		Иначе
			НС.Номенклатура = Объект.Автомобиль;
		КонецЕсли;
		НС.Количество = 1;
		НС.СчетУчета = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,Справочники.Номенклатура.ПустаяСсылка()).СчетУчета;
		НС.СчетУчетаНУ = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,Справочники.Номенклатура.ПустаяСсылка()).СчетУчета;
		НС.СчетУчетаНДС = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект.Ссылка,Справочники.Номенклатура.ПустаяСсылка()).СчетУчетаНДС;
		
		
		ВходящийДанные=Новый Структура;
		ВходящийДанные.Вставить("Товары",ТаблицаТовары);
		ВходящийДанные.Вставить("СИзмененеимНоменклатуры",СтараяСтатьяИзменениеВин или ХозОперацияИзменениеВин);
		ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящийДанные,,"ПоступлениеДопРасходовАвтомобилиВСебестоимость");
		
		
	Иначе	
		
		ВыгрузитьПоПравилу(Объект.Ссылка,,,,"ПоступлениеДопРасходовАвтомобилиНеВСебестоимость");
		
	КонецЕсли;
	СчетФактура = Неопределено;
	МассивВидов = Новый Массив(1); 
	МассивВидов[0] = "СчетФактураПолученный";
	МассивПодчиненных = дкПолучитьМассивПодчиненных(Объект.Ссылка, МассивВидов, Истина);
	Если НЕ обЗначениеНеЗаполнено(МассивПодчиненных) И МассивПодчиненных.Количество()&gt;0 Тогда
		СчетФактура = МассивПодчиненных[0].Ссылка;
		ВыгрузитьПоПравилу(СчетФактура,,,,"СчетФактураПолученныйТовары");
	КонецЕсли;
КонецЕсли;</Текст>
		</Алгоритм>
		<Алгоритм Имя="ВыгрузитьЗНизПКО" ИспользуетсяПриЗагрузке="false">
			<Текст>ЭтоАСС = ЗаказНаряд.Организация.ИНН = "5262158548";

ТипКлиента = ЗаказНаряд.Контрагент.ТипКлиента;
ВидОтношений = ЗаказНаряд.Контрагент.ВидКонтрагента;

ТипКлиентаГК = (ТипКлиента = Справочники.ТипыКлиентов.ЮридическоеЛицоГК или ТипКлиента = Справочники.ТипыКлиентов.ЮридическоеЛицоГКАгатИПартнеры);
ВидОтношенийВнутренний = (ВидОтношений = Перечисления.ВидыКонтрагентов.ВнутреннееПодразделение);

КодУпр1 = ЗаказНаряд.ВидРемонта.КодУпр = 1; //ГАРАНТИЙНЫЙ РЕМОНТ
КодУпр2 = ЗаказНаряд.ВидРемонта.КодУпр = 2; //ГАРАНТИЙНЫЙ РЕМОНТ ДОБРОЙ ВОЛИ 
КодУпр3 = ЗаказНаряд.ВидРемонта.КодУпр = 3; //КОММЕРЧЕСКИЙ РЕМОНТ 
КодУпр4 = ЗаказНаряд.ВидРемонта.КодУпр = 4; //РЕМОНТ И ОБСЛУЖИВАНИЕ СОБСТВЕННОГО АВТОМОБИЛЯ 
КодУпр5 = ЗаказНаряд.ВидРемонта.КодУпр = 5; //РЕМОНТ ТОВАРНОГО АВТОМОБИЛЯ 
КодУпр6 = ЗаказНаряд.ВидРемонта.КодУпр = 6; //КУЗОВНОЙ РЕМОНТ СТОРОННИЙ 
КодУпр7 = ЗаказНаряд.ВидРемонта.КодУпр = 7; //КУЗОВНОЙ РЕМОНТ СОБСТВЕННЫЙ 
КодУпр8 = ЗаказНаряд.ВидРемонта.КодУпр = 8; //РЕКЛАМАЦИЯ 
КодУпр9 = ЗаказНаряд.ВидРемонта.КодУпр = 9; //ПРЕДПРОДАЖНОЕ СЕРВИСНОЕ ОБСЛУЖИВАНИЕ 
КодУпр10 = ЗаказНаряд.ВидРемонта.КодУпр = 10; //УСТАНОВКА ДОПОЛНИТЕЛЬНОГО ОБОРУДОВАНИЯ ЗА СЧЕТ КЛИЕНТА 
КодУпр11 = ЗаказНаряд.ВидРемонта.КодУпр = 11; //УСТАНОВКА ДОПОЛНИТЕЛЬНОГО ОБОРУДОВАНИЯ ЗА СЧЕТ САЛОНА
КодУпр12 = ЗаказНаряд.ВидРемонта.КодУпр = 12; //УСТАНОВКА ДОПОЛНИТЕЛЬНОГО ОБОРУДОВАНИЯ В СТОИМОСТИ АВТОМОБИЛЯ 
КодУпр13 = ЗаказНаряд.ВидРемонта.КодУпр = 13; //ЛЮКС ТОВАРНОГО АВТОМОБИЛЯ ВНУТРЕННИЙ
КодУпр14 = ЗаказНаряд.ВидРемонта.КодУпр = 14; //ТЕХНИЧЕСКОЕ ОБСЛУЖИВАНИЕ 
КодУпр15 = ЗаказНаряд.ВидРемонта.КодУпр = 15; //ТЕХНИЧЕСКОЕ ОБСЛУЖИВАНИЕ НЕВОЗМЕЩАЕМОЕ 

Отказ = КодУпр13;
НужноРазбитьРеализацию = Ложь;
Если КодУпр1 Тогда
	
	Если Не ТипКлиентаГК Тогда
		НеНужнаРеализация = Истина;
	Иначе
		НеНужнаРеализация = Ложь;
	КонецЕсли;
	Если ЭтоАСС Тогда
		НеНужнаРеализация = Ложь;
		НужноРазбитьРеализацию = Ложь;
	КонецЕсли;
	
ИначеЕсли КодУпр2 ИЛИ КодУпр8 ИЛИ КодУпр15 Тогда
	
	НеНужнаРеализация = Истина;
	
ИначеЕсли КодУпр4 ИЛИ КодУпр5 ИЛИ КодУпр9 ИЛИ КодУпр11 ИЛИ КодУпр12 Тогда
	Если ТипКлиентаГК и ВидОтношенийВнутренний Тогда
		НеНужнаРеализация = Истина;
	Иначе
		НеНужнаРеализация = Ложь;
	КонецЕсли;
	Если ЭтоАСС Тогда
		НужноРазбитьРеализацию = Истина;
	Иначе
		НужноРазбитьРеализацию = Ложь;
	КонецЕсли;

Иначе
	
	НеНужнаРеализация = Ложь;
	Если ЭтоАСС Тогда
		НужноРазбитьРеализацию = Истина;
	КонецЕсли;
		
	
КонецЕсли;
		
//НеНужнаРеализация = Истина;

Если не Отказ Тогда
	Если НЕ ЭтоАСС Тогда
		
		Если НЕ НеНужнаРеализация Тогда
			//Значение = ЗаказНаряд.Ссылка;
			ИмяПКО = "ЗаказНаряд";
		Иначе
			Пусто = Истина;
			Отказ = Истина;
			//Значение = Документы.ЗаказНаряд.ПустаяСсылка();
		КонецЕсли;
		
	Иначе //Скания
		Запрос=новый Запрос;
		Запрос.Текст="ВЫБРАТЬ
		             |	ЗаказНарядТовары.Ссылка,
		             |	ЗаказНарядТовары.Номенклатура,
					 |	ЗаказНарядТовары.Номенклатура.БазоваяЕдиницаИзмерения КАК ЕдиницаИзмерения,
		             |	ЗаказНарядТовары.Количество,
		             |	ЗаказНарядТовары.ЕдиницаИзмерения,
		             |	ЗаказНарядТовары.Коэффициент,
		             |	ЗаказНарядТовары.Цена,
		             |	ЗаказНарядТовары.Сумма,
		             |	ЗаказНарядТовары.СтавкаНДС,
