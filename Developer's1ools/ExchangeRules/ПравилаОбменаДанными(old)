	|	ПоступлениеТоваровУслугТовары.СчетРасчетов
	|ИЗ
	|	Документ.ПоступлениеТоваровУслуг.Товары КАК ПоступлениеТоваровУслугТовары
	|ГДЕ
	|	ПоступлениеТоваровУслугТовары.Ссылка = &amp;Ссылка";
		
		
	Запр.УстановитьПараметр("Авто", Объект.Товары[0].Номенклатура);
	Запр.УстановитьПараметр("Орг", Объект.Организация);
	Запр.УстановитьПараметр("Счет",ПланыСчетов.Хозрасчетный.ТоварыНаСкладах);
	   
	Запр.УстановитьПараметр("Дата", Новый Граница(Объект.Дата, ВидГраницы.Исключая));
	РезЗапроса = Запр.Выполнить();
	СуммаДоИзменения = 0;
	Сумма = Объект.Товары[0].Сумма;
	СтавкаНДС = Перечисления.СтавкиНДС.НДС18;
	СуммаНДСДоИЗменения = 0;
	СуммаНДС = 0;
	Автомобиль =  Объект.Товары[0].Номенклатура;
	Если НЕ РезЗапроса.Пустой() Тогда
		Объект.Товары.Очистить();
		Выборка = РезЗапроса.Выбрать();
		Выборка.Следующий();
		Объект.ДокументПоступления = Выборка.ДокументПоступления;
		Запр1.УстановитьПараметр("Ссылка",Выборка.ДокументПоступления);
		Объект.Склад = Выборка.Склад;
		Результат1 = Запр1.Выполнить().Выбрать();
		Пока Результат1.Следующий() Цикл
			
			СтрокаТоваров = Объект.Товары.Добавить();
			
			ЗаполнитьЗначенияСвойств(СтрокаТоваров, Результат1);
			
			СтрокаТоваров.Сумма = -Сумма+СтрокаТоваров.Сумма;
			
			СтрокаТоваров.Цена = СтрокаТоваров.Сумма;
			
			СтрокаТоваров.СуммаНДС = (СтрокаТоваров.Сумма*18)/118;
			
		КонецЦикла;
		
	Иначе
		Сообщить("Документ не найден!");
		Объект.Проведен = Ложь;
		Объект.Записать();
	КонецЕсли;

	Если Объект.Проведен Тогда
		Объект.ПометкаУдаления = Ложь;
	КонецЕсли;
	Объект.СуммаВключаетНДС =Истина;
	Объект.Записать();

	Если Объект.ДокументПоступления &lt;&gt; Неопределено Тогда //нет смысла автозаполнения
		СчетФактура = УчетНДСПереопределяемый.НайтиПодчиненныйСчетФактуруПолученный(Объект.Ссылка);
		Если (НЕ ЗначениеЗаполнено(СчетФактура)) Тогда
			СчетФактура=Документы.СчетФактураПолученный.СоздатьДокумент();
			СчетФактура.ДокументыОснования.Очистить();
			//СчетФактура.ЗаполнитьПоДокументуОснованию(Объект.Ссылка);
			
			СчетФактура.ДокументыОснования.Добавить().ДокументОснование = Объект.Ссылка;
				 
						СчетФактура.ОпределениеПараметровСчетаФактуры();
			СчетФактура.Проведен = Объект.Ссылка.Проведен;
			СчетФактура.Записать();
		КонецЕсли;
	КонецЕсли;


Выполнить(Алгоритмы.ПроставитьВалютуДокумента);

