					<КодПравилаКонвертации>ПриходныйКассовыйОрдер                            </КодПравилаКонвертации>
					<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
					<ОбъектВыборки>ДокументСсылка.ПриходныйКассовыйОрдер</ОбъектВыборки>
					<ПередВыгрузкойОбъекта>Если Не Константы.ЦентральнаяБазаСтрахования.Получить() и Объект.ПодразделениеКомпании.ЭтоПодразделениеСтрахования Тогда
	Отказ = Истина;
КонецЕсли;

Если Объект.КассаКомпании.ДляОплатыПоКарточкам Тогда
	ИмяПКО = "ПриходныйКассовыйОрдерПоКартам";
Иначе
	ИмяПКО = "ПриходныйКассовыйОрдер";
КонецЕсли;</ПередВыгрузкойОбъекта>
				</Правило>
				<Правило Отключить="false">
					<Код>РасходныйКассовыйОрдер</Код>
					<Наименование>Расходный кассовый ордер</Наименование>
					<Порядок>50</Порядок>
					<КодПравилаКонвертации>РасходныйКассовыйОрдер                            </КодПравилаКонвертации>
					<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
					<ОбъектВыборки>ДокументСсылка.РасходныйКассовыйОрдер</ОбъектВыборки>
					<ПередВыгрузкойОбъекта>Если Не Константы.ЦентральнаяБазаСтрахования.Получить() и Объект.ПодразделениеКомпании.ЭтоПодразделениеСтрахования Тогда
	Отказ = Истина;
КонецЕсли;

Если Объект.КассаКомпании.ДляОплатыПоКарточкам Тогда
	ИмяПКО = "РасходныйКассовыйОрдерПоКартам";
Иначе
	ИмяПКО = "РасходныйКассовыйОрдер";
КонецЕсли;</ПередВыгрузкойОбъекта>
				</Правило>
				<Правило Отключить="false">
					<Код>АвансовыйОтчет</Код>
					<Наименование>Авансовый отчет</Наименование>
					<Порядок>400</Порядок>
					<КодПравилаКонвертации>АвансовыйОтчет                                    </КодПравилаКонвертации>
					<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
					<ОбъектВыборки>ДокументСсылка.АвансовыйОтчет</ОбъектВыборки>
					<ПередВыгрузкойОбъекта>ИсходящиеДанные  = Новый Структура;

ТЗТовары = Новый ТаблицаЗначений;
ТЗТовары.Колонки.Добавить("ВидДокВходящий");
ТЗТовары.Колонки.Добавить("ДатаВходящегоДокумента");
ТЗТовары.Колонки.Добавить("Количество");
ТЗТовары.Колонки.Добавить("Номенклатура");
ТЗТовары.Колонки.Добавить("НомерВходящегоДокумента");
ТЗТовары.Колонки.Добавить("СтавкаНДС");
ТЗТовары.Колонки.Добавить("Сумма");
ТЗТовары.Колонки.Добавить("СуммаНДС");
ТЗТовары.Колонки.Добавить("Цена");
ТЗТовары.Колонки.Добавить("СчетУчета");
ТЗТовары.Колонки.Добавить("СчетУчетаНДС");
Склад = Объект.СкладКомпании;
Для Каждого Строка ИЗ Объект.Товары Цикл
	попытка
		Если Строка.Номенклатура.ТипНоменклатурыУправленческий = Справочники.ТипыНоменклатурыУправленческий.СторонниеУслугиИРаботы Тогда
			Продолжить;
		КонецЕсли;
		НС = ТЗТовары.Добавить();
		//НС.ВидДокВходящий=Строка.ДокументРасхода;
		//НС.ДатаВходящегоДокумента=Строка.ДатаРасхода;
		//НС.НомерВходящегоДокумента=Строка.НомерДокументаРасхода;
		НС.Количество=Строка.Количество;
		НС.Номенклатура=Строка.Номенклатура;
		Если Не ЗначениеЗаполнено(Строка.СтавкаНДС) Тогда
			СтавкаНДС="БезНДС";
		ИначеЕсли Строка.СтавкаНДС.Ставка=18 Тогда
			СтавкаНДС="НДС18";
		Иначе 
			СтавкаНДС="БезНДС";
		КонецЕсли;
		НС.СтавкаНДС=СтавкаНДС;
		НС.Сумма=Строка.Сумма;
		НС.СуммаНДС=Строка.СуммаНДС;
		НС.Цена=Строка.Цена;
		НС.СчетУчета=БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект,Строка.Номенклатура).СчетУчета;
		НС.СчетУчетаНДС=БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект,Строка.Номенклатура).СчетУчетаНДС;
	исключение 
	конецпопытки;
КонецЦикла;

ТЗПрочие = Новый ТаблицаЗначений;
ТЗПрочие.Колонки.Добавить("ВидДокВходящий");
ТЗПрочие.Колонки.Добавить("ДатаВходящегоДокумента");
ТЗПрочие.Колонки.Добавить("ПодразделениеЗатрат");
ТЗПрочие.Колонки.Добавить("Содержание");
ТЗПрочие.Колонки.Добавить("Поставщик");
ТЗПрочие.Колонки.Добавить("СтавкаНДС");
ТЗПрочие.Колонки.Добавить("СчетЗатрат");
ТЗПрочие.Колонки.Добавить("СчетЗатратНУ");
ТЗПрочие.Колонки.Добавить("Субконто1");
ТЗПрочие.Колонки.Добавить("Субконто2");
ТЗПрочие.Колонки.Добавить("Субконто3");
ТЗПрочие.Колонки.Добавить("СубконтоНУ1");
ТЗПрочие.Колонки.Добавить("СубконтоНУ2");
ТЗПрочие.Колонки.Добавить("СубконтоНУ3");
ТЗПрочие.Колонки.Добавить("Сумма");
ТЗПрочие.Колонки.Добавить("СчетУчетаНДС");

Для Каждого Строка ИЗ Объект.Товары Цикл
	попытка
		Если Строка.Номенклатура.ТипНоменклатурыУправленческий = Справочники.ТипыНоменклатурыУправленческий.СторонниеУслугиИРаботы Тогда
			НС = ТЗПрочие.Добавить();
			//НС.ВидДокВходящий=Строка.ДокументРасхода;
			//НС.ДатаВходящегоДокумента=Строка.ДатаРасхода;
			//НС.ПодразделениеЗатрат=Строка.ЦФОПотребитель;
			НС.Содержание=Строка.Номенклатура.Наименование;
			НС.Поставщик=Справочники.Контрагенты.ПустаяСсылка();//Строка.Контрагент;
			Если Не ЗначениеЗаполнено(Строка.СтавкаНДС) Тогда
				СтавкаНДС="БезНДС";
			ИначеЕсли Строка.СтавкаНДС.Ставка=18 Тогда
				СтавкаНДС="НДС18";
			Иначе 
				СтавкаНДС="БезНДС";
			КонецЕсли;
			НС.СтавкаНДС=СтавкаНДС;
			Попытка
				НС.СчетЗатрат = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект,Строка.Номенклатура).СчетУчета;
			Исключение
			КонецПопытки;
			НС.СчетЗатратНУ= НС.СчетЗатрат;
			СчетЗатрат = НС.СчетЗатрат;
			ЧислоАктивныхСубконто = СчетЗатрат.ВидыСубконто.Количество();
			Субконто1 = "";
			Субконто2 = "";
			Субконто3= "";
			Если ЧислоАктивныхСубконто &gt;= 1 Тогда
				Попытка
					Субконто1 = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект,Строка.Номенклатура).Субконто1;
				Исключение
				КонецПопытки;
			КонецЕсли;
			Если ЧислоАктивныхСубконто &gt;= 2 Тогда
				Попытка
					Субконто2 = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект,Строка.Номенклатура).Субконто2;
				Исключение
				КонецПопытки;
			КонецЕсли;
			Если ЧислоАктивныхСубконто &gt;= 3 Тогда
				Попытка
					Субконто3 = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект,Строка.Номенклатура).Субконто3;
				Исключение
				КонецПопытки;
			КонецЕсли;
			НС.Субконто1 = Субконто1;
			НС.Субконто2 = Субконто2;
			НС.Субконто3= Субконто3;
			НС.СубконтоНУ1= Субконто1;
			НС.СубконтоНУ2= Субконто2;
			НС.СубконтоНУ3= Субконто3;
			НС.Сумма = Строка.Сумма;
			НС.СчетУчетаНДС=БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект,Строка.Номенклатура).СчетУчетаНДС;
		КонецЕсли;
	исключение 
	конецпопытки;
КонецЦикла;

Для Каждого Строка ИЗ Объект.Расходы Цикл
	
	Если Строка.НаРасчеты Тогда
		Продолжить;
	КонецЕсли;
	НС = ТЗПрочие.Добавить();
	НС.ВидДокВходящий=Строка.ДокументРасхода;
	НС.ДатаВходящегоДокумента=Строка.ДатаРасхода;
	НС.ПодразделениеЗатрат=Строка.ЦФОПотребитель;
	НС.Содержание=Строка.Содержание;
	НС.Поставщик=Справочники.Контрагенты.ПустаяСсылка();
	СтавкаНДС="БезНДС";
	НС.СтавкаНДС=СтавкаНДС;
	Попытка
		НС.СчетЗатрат = БухгалтерскийУчет.ПолучитьСоответсвиеУУиБУ(Объект,Строка.СтатьяДоходовИРасходовДляОбмена).СчетЗатрат;
	Исключение
	КонецПопытки;
	Попытка
		НС.СчетЗатратНУ= НС.СчетЗатрат;
	Исключение
	КонецПопытки;
	Попытка
		НС.Субконто1 = БухгалтерскийУчет.ПолучитьСоответсвиеУУиБУ(Объект,Строка.СтатьяДоходовИРасходовДляОбмена).СтатьяЗатрат;
	Исключение
	КонецПопытки;
	Попытка
		НС.Субконто2 = БухгалтерскийУчет.ПолучитьСоответсвиеУУиБУ(Объект,Строка.СтатьяДоходовИРасходовДляОбмена).ПрочиеДоходыИРасходы;
	Исключение
	КонецПопытки;
	Попытка
		НС.Субконто3 = БухгалтерскийУчет.ПолучитьСоответсвиеУУиБУ(Объект,Строка.СтатьяДоходовИРасходовДляОбмена).НоменклатурнаяГруппа;
	Исключение
	КонецПопытки;
	Попытка
		НС.СубконтоНУ1= НС.Субконто1;
	Исключение
	КонецПопытки;
	Попытка
		НС.СубконтоНУ2= НС.Субконто2;
	Исключение
	КонецПопытки;
	Попытка
		НС.СубконтоНУ3= НС.Субконто3;
	Исключение
	КонецПопытки;
	НС.Сумма = Строка.Сумма;
	Попытка
		НС.СчетУчетаНДС = БухгалтерскийУчет.ПолучитьСчетаУчетаНоменклатуры(Объект, Справочники.Номенклатура.ПустаяСсылка()).СчетУчетаНДС;
	Исключение
	КонецПопытки;
КонецЦикла;


ИсходящиеДанные.Вставить("Товары",ТЗТовары);
ИсходящиеДанные.Вставить("Прочее",ТЗПрочие);
ИсходящиеДанные.Вставить("Склад",Склад);</ПередВыгрузкойОбъекта>
				</Правило>
			</Группа>
