Партия = Документы.ПоступлениеАвтомобилей.ПустаяСсылка();

Значение = БухгалтерскийУчет.ПолучитьНастрокуБУПоВидуРемонта(Источник.ВидРемонта,Источник).СубконтоЗатрат3;

СчетЗатрат = БухгалтерскийУчет.ПолучитьНастрокуБУПоВидуРемонта(Источник.ВидРемонта,Источник).СчетУчетаЗатрат;

Если ЗначениеЗаполнено(СчетЗатрат) Тогда
	Если СчетЗатрат.ВидыСубконто.Количество() &gt; 2 Тогда
		Выполнить(Алгоритмы.ПолучитьОстатокАвто);
		Если СчетЗатрат.ВидыСубконто[2].ВидСубконто = ПланыВидовХарактеристик.ВидыСубконтоРегламентированные.Партии Тогда
			Значение = Партия;
			Если ТипЗнч(Партия) = Тип("ДокументСсылка.ПоступлениеАвтомобилей") Тогда
				Если Значение.ХозОперация = Справочники.ХозОперации.ПоступлениеАвтомобилейОтветственноеХранение Тогда
					ИмяПКО = "ПоступлениеАвтомобилей";
				ИначеЕсли Значение.ХозОперация = Справочники.ХозОперации.ПереходВСобственностьАвтомобилей Тогда
					ВыгрузитьПоПравилу(Значение, , , , "ВозвратАвтомобиляИзХранения");
					ИмяПКО = "ПоступлениеАвтомобилей";
				Иначе
					ИмяПКО = "ПоступлениеАвтомобилей";
				КонецЕсли;
			Иначе 
				Значение = Документы.ПоступлениеАвтомобилей.ПустаяСсылка(); 
			КонецЕсли;
        ИначеЕсли СчетЗатрат.ВидыСубконто[2].ВидСубконто = ПланыВидовХарактеристик.ВидыСубконтоРегламентированные.Склады Тогда
			Значение = Склад; 
		КонецЕсли;
	КонецЕсли;
КонецЕсли;</ПриВыгрузке>
