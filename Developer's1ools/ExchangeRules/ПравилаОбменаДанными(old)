							<Приемник Имя="НДСвСтоимостиТоваров" Вид="Реквизит" Тип="ПеречислениеСсылка.ДействиеНДСВСтоимостиТоваров"/>
							<ПриВыгрузке>ВидРемонта = Справочники.ВидыРемонта.ВнутренняяМойка;

Попытка
	Настройка = БухгалтерскийУчет.ПолучитьНастрокуБУПоВидуРемонта (ВидРемонта, Источник.Ссылка).НДСВстоимости;
	Если Настройка = Перечисления.ДействиеНДСВСтоимостиТоваров.ВключитьВСтоимость Тогда
		Значение = "ВключитьВСтоимость";
	ИначеЕсли Настройка = Перечисления.ДействиеНДСВСтоимостиТоваров.ИсключитьИзСтоимости Тогда
		Значение = "ИсключитьИзСтоимости";
	Иначе
		Значение = "НеИзменять";
	КонецЕсли;
Исключение
	Значение = "НеИзменять";
КонецПопытки;</ПриВыгрузке>
						</Свойство>
						<Свойство>
							<Код>8</Код>
							<Наименование>--&gt; СчетЗатрат</Наименование>
							<Порядок>400</Порядок>
							<Источник Имя="" Вид=""/>
							<Приемник Имя="СчетЗатрат" Вид="Реквизит" Тип="ПланСчетовСсылка.Хозрасчетный"/>
