					</Правило>
				</Группа>
				<Группа Отключить="false">
					<Код>РеализацииАвтомобилей</Код>
					<Наименование>РеализацииАвтомобилей</Наименование>
					<Порядок>50</Порядок>
					<Правило Отключить="false">
						<Код>РеализацияАвтомобилей</Код>
						<Наименование>Реализация автомобилей</Наименование>
						<Порядок>50</Порядок>
						<КодПравилаКонвертации>РеализацияАвтомобилей                             </КодПравилаКонвертации>
						<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
						<ОбъектВыборки>ДокументСсылка.РеализацияАвтомобилей</ОбъектВыборки>
						<ПередВыгрузкойОбъекта>Если Не Отказ Тогда
	СчетФактура = Неопределено;
	МассивВидов = Новый Массив(1); 
	МассивВидов[0] = "СчетФактураВыданный";
	МассивПодчиненных = дкПолучитьМассивПодчиненных(Объект.Ссылка, МассивВидов, Истина);
	Если НЕ обЗначениеНеЗаполнено(МассивПодчиненных) И МассивПодчиненных.Количество()&gt;0 Тогда
		СчетФактура = МассивПодчиненных[0].Ссылка;
		ВыгрузитьПоПравилу(СчетФактура,,,,"СчетФактураВыданныйАвтомобили");
	КонецЕсли;
КонецЕсли;</ПередВыгрузкойОбъекта>
					</Правило>
					<Правило Отключить="false">
						<Код>ВозвратОтПокупателяАвтомобилей</Код>
						<Наименование>ВозвратОтПокупателяАвтомобилей</Наименование>
						<Порядок>50</Порядок>
						<КодПравилаКонвертации>ВозвратОтПокупателяАвтомобилей                    </КодПравилаКонвертации>
						<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
						<ОбъектВыборки>ДокументСсылка.ВозвратОтПокупателяАвтомобилей</ОбъектВыборки>
						<ПередВыгрузкойОбъекта>Если Не Отказ Тогда
	СчетФактура = Неопределено;
	МассивВидов = Новый Массив(1); 
	МассивВидов[0] = "СчетФактураВыданный";
	МассивПодчиненных = дкПолучитьМассивПодчиненных(Объект.Ссылка, МассивВидов, Истина);
	Если НЕ обЗначениеНеЗаполнено(МассивПодчиненных) И МассивПодчиненных.Количество()&gt;0 Тогда
		СчетФактура = МассивПодчиненных[0].Ссылка;
		ВыгрузитьПоПравилу(СчетФактура,,,,"СчетФактураВыданныйАвтомобили");
	КонецЕсли;
КонецЕсли;</ПередВыгрузкойОбъекта>
					</Правило>
				</Группа>
			</Группа>
			<Группа Отключить="false">
				<Код>Мойка</Код>
				<Наименование>Мойка</Наименование>
				<Порядок>300</Порядок>
				<Правило Отключить="false">
					<Код>АктОбОказанииУслугМойки</Код>
					<Наименование>Акт об оказании услуг мойки</Наименование>
					<Порядок>50</Порядок>
					<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
