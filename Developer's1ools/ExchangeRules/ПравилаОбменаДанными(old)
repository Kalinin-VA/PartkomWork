					<ПередВыгрузкойОбъекта>Если Объект.ХозОперация = Справочники.ХозОперации.ПоступлениеДенежныхДокументов Тогда
	ИмяПКО = "ПоступлениеДенежныхДокументов";
Иначе
	ИмяПКО = "ВыдачаДенежныхДокументов";
КонецЕсли;</ПередВыгрузкойОбъекта>
				</Правило>
			</Группа>
			<Группа Отключить="false">
				<Код>Банк</Код>
				<Наименование>Банк</Наименование>
				<Порядок>500</Порядок>
				<Правило Отключить="true">
					<Код>Выписка</Код>
					<Наименование>Банковская выписка</Наименование>
					<Порядок>550</Порядок>
					<СпособОтбораДанных>СтандартнаяВыборка</СпособОтбораДанных>
					<ОбъектВыборки>ДокументСсылка.Выписка</ОбъектВыборки>
					<ПередВыгрузкойОбъекта>Состав = Объект.Состав.Выгрузить();
Стр = Объект.Состав[0];
Если НЕ Объект.Состав.Итог("СуммаПриход") = 0 и НЕ Объект.Состав.Итог("СуммаРасход") = 0 Тогда
	
Иначе
	Если Состав.Количество() &gt; 0 Тогда
		Если ЗначениеЗаполнено(Стр.СуммаПриход) Тогда
			ВходящийДанные=Новый Структура;
			ВходящийДанные.Вставить("РасшифровкаПлатежа",Состав);
			ВходящийДанные.Вставить("ВидОперации",Стр.СтатьяДвиженияДенежныхСредств.СтатьяДДСБУ);
			ВходящийДанные.Вставить("СтатьяДвиженияДенежныхСредств",Стр.СтатьяДвиженияДенежныхСредств.СтатьяДДСБУ);
			ВходящийДанные.Вставить("СуммаДокумента", Стр.СуммаПриход);
			ВходящийДанные.Вставить("Контрагент",Стр.Контрагент);
			ВходящийДанные.Вставить("СчетКонтрагента",Стр.БанковскийСчетКонтрагента);
			ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящийДанные,,"ПоступлениеНаРасчетныйСчет");
		ИначеЕсли ЗначениеЗаполнено(Стр.СуммаРасход) Тогда
			ВходящийДанные=Новый Структура;
			ВходящийДанные.Вставить("РасшифровкаПлатежа",Состав);
			ВходящийДанные.Вставить("ВидОперации",Стр.СтатьяДвиженияДенежныхСредств.СтатьяДДСБУ);
			ВходящийДанные.Вставить("СтатьяДвиженияДенежныхСредств",Стр.СтатьяДвиженияДенежныхСредств.СтатьяДДСБУ);
			ВходящийДанные.Вставить("СуммаДокумента", Стр.СуммаРасход);
			ВходящийДанные.Вставить("Контрагент",Стр.Контрагент);
			ВходящийДанные.Вставить("СчетКонтрагента",Стр.БанковскийСчетКонтрагента);
			ВыгрузитьПоПравилу(Объект.Ссылка,,ВходящийДанные,,"СписаниеСРасчетногоСчета");
		КонецЕсли;
	КонецЕсли;
КонецЕсли;
Отказ = Истина;</ПередВыгрузкойОбъекта>
				</Правило>
			</Группа>
			<Группа Отключить="false">
				<Код>КорректировкиПоступлений</Код>
