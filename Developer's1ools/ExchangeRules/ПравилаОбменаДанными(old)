								<Источник Имя="" Вид=""/>
								<Приемник Имя="ПокупателемВыставляетсяСчетФактураНаВозврат" Вид="Реквизит" Тип="Булево"/>
								<ПередВыгрузкой>СчетФактура = Неопределено;
МассивВидов = Новый Массив(1); 
МассивВидов[0] = "СчетФактураПолученный";
МассивПодчиненных = дкПолучитьМассивПодчиненных(Источник.Ссылка, МассивВидов, Истина);
Если ЗначениеЗаполнено(МассивПодчиненных) И МассивПодчиненных.Количество()&gt;0 Тогда
	СчетФактура = МассивПодчиненных[0].Ссылка;
	Значение = Истина;
