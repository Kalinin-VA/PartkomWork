{"S","
МаксимальнаяДлинаПути =6;
//сНоменклатура =ОбъектДляПравилВыгрузки.Ссылка;
 Пролог =  ""ВЫБРАТЬ
     |	НоменклатураИзменения.Ссылка
     |ПОМЕСТИТЬ ИзмНоменклатура
     |ИЗ
     |	Справочник.Номенклатура.Изменения КАК НоменклатураИзменения
     |ГДЕ
     |	НоменклатураИзменения.Узел = &Узел И
	 |	НоменклатураИзменения.Ссылка.ЭтоГруппа = ЛОЖЬ 
	 //|  И НоменклатураИзменения.Ссылка = &ОбъектДляПравилВыгрузки
    // |	И НоменклатураИзменения.Ссылка  В(&МассивНоменклатуры)
     |;
     |                                                    
	 | ВЫБРАТЬ
	 |	Номенклатура.Родитель КАК НачалоДуги,
	 |	Номенклатура.Ссылка КАК КонецДуги
	 | ПОМЕСТИТЬ ЗамыканияДлины1
	 | ИЗ
	 |	Справочник.Номенклатура КАК Номенклатура
	 | ГДЕ
	 |	Номенклатура.Родитель <> ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)
	 |
	 | ОБЪЕДИНИТЬ
	 |
	 | ВЫБРАТЬ
	 |	Номенклатура.Ссылка,
	 |	Номенклатура.Ссылка
	 | ИЗ
	 |	ИзмНоменклатура КАК Номенклатура
	 | ГДЕ
	 |	Номенклатура.Ссылка.ЭтоГруппа = ЛОЖЬ ; "" ;

	 //|  и Номенклатура.Ссылка = &ОбъектДляПравилВыгрузки 

//Конецесли;


    Рефрен = ""ВЫБРАТЬ РАЗЛИЧНЫЕ ПерваяДуга.НачалоДуги, ВтораяДуга.КонецДуги ПОМЕСТИТЬ ЗамыканияДлины#2 ИЗ ЗамыканияДлины#1 КАК ПерваяДуга
            | СОЕДИНЕНИЕ ЗамыканияДлины#1 КАК ВтораяДуга ПО ПерваяДуга.КонецДуги = ВтораяДуга.НачалоДуги;
            | УНИЧТОЖИТЬ ЗамыканияДлины#1; "";

    Эпилог = ""ВЫБРАТЬ  КонецДуги Номенклатура, НачалоДуги СегментНоменклатуры, Истина Активность  ИЗ ЗамыканияДлины#2 Где НачалоДуги<>КонецДуги УПОРЯДОЧИТЬ ПО Номенклатура"";

	
	ТекстЗапроса = Пролог;
	
    МаксимальнаяДлинаЗамыканий = 1;

    Пока МаксимальнаяДлинаЗамыканий < МаксимальнаяДлинаПути Цикл

        ТекстЗапроса = ТекстЗапроса + СтрЗаменить(СтрЗаменить(Рефрен, ""#1"", Формат(МаксимальнаяДлинаЗамыканий, ""ЧГ=0"")), ""#2"", Формат(2 * МаксимальнаяДлинаЗамыканий, ""ЧГ=0""));

        МаксимальнаяДлинаЗамыканий = 2 * МаксимальнаяДлинаЗамыканий ;

    КонецЦикла;

    ТекстЗапроса = ТекстЗапроса + СтрЗаменить(Эпилог, ""#2"", Формат(МаксимальнаяДлинаЗамыканий, ""ЧГ=0""));
Запрос = Новый Запрос(ТекстЗапроса);
Запрос.УстановитьПараметр(""Узел"",параметры.пУзелОбмена);
//Запрос.УстановитьПараметр(""Номенклатура"",Источник.Ссылка);

Запрос.УстановитьПараметр(""МассивНоменклатуры"",параметры.пМассивНоменклатуры);
//Запрос.УстановитьПараметр(""ОбъектДляПравилВыгрузки"",сНоменклатура);
//Если cНоменклатура <> неопределено тогда
//	 Запрос.УстановитьПараметр(""сНоменклатура"",cНоменклатура);
//конецесли;	                                     
ВыборкаДанных =  Запрос.Выполнить();
"},
